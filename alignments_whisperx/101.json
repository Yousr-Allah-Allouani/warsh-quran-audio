  "warsh_ayahs": [
    {
      "ayah_number": 1,
      "text": "اِ۬لْقَارِعَةُ مَا اَ۬لْقَارِعَةُۖ"
    },
    {
      "ayah_number": 2,
      "text": "وَمَآ أَدْر۪يٰكَ مَا اَ۬لْقَارِعَةُۖ"
    },
    {
      "ayah_number": 3,
      "text": "يَوْمَ يَكُونُ اُ۬لنَّاسُ كَالْفَرَاشِ اِ۬لْمَبْثُوثِ"
    },
    {
      "ayah_number": 4,
      "text": "وَتَكُونُ اُ۬لْجِبَالُ كَالْعِهْنِ اِ۬لْمَنفُوشِۖ"
    },
    {
      "ayah_number": 5,
      "text": "فَأَمَّا مَن ثَقُلَتْ مَوَٰزِينُهُۥ"
    },
    {
      "ayah_number": 6,
      "text": "فَهُوَ فِے عِيشَةٖ رَّاضِيَةٖۖ"
    },
    {
      "ayah_number": 7,
      "text": "وَأَمَّا مَنْ خَفَّتْ مَوَٰزِينُهُۥ"
    },
    {
      "ayah_number": 8,
      "text": "فَأُمُّهُۥ هَاوِيَةٞۖ"
    },
    {
      "ayah_number": 9,
      "text": "وَمَآ أَدْر۪يٰكَ مَا هِيَهْۖ"
    },
    {
      "ayah_number": 10,
      "text": "نَارٌ حَامِيَةٌۖ"
    }
  ]
}