{
  "surah_number": 37,
  "name": "اُ۬لصَّٰٓفَّٰتِ",
  "number_of_ayahs": 182,
  "ayahs": [
    {
      "ayah_number": 1,
      "text": "وَالصَّٰٓفَّٰتِ صَفّاٗ"
    },
    {
      "ayah_number": 2,
      "text": "فَالزَّٰجِرَٰتِ زَجْراٗ"
    },
    {
      "ayah_number": 3,
      "text": "فَالتَّٰلِيَٰتِ ذِكْراً"
    },
    {
      "ayah_number": 4,
      "text": "اِنَّ إِلَٰهَكُمْ لَوَٰحِدٞۖ"
    },
    {
      "ayah_number": 5,
      "text": "رَّبُّ اُ۬لسَّمَٰوَٰتِ وَالَارْضِ وَمَا بَيْنَهُمَا وَرَبُّ اُ۬لْمَشَٰرِقِۖ"
    },
    {
      "ayah_number": 6,
      "text": "إِنَّا زَيَّنَّا اَ۬لسَّمَآءَ اَ۬لدُّنْي۪ا بِزِينَةِ اِ۬لْكَوَاكِبِۖ"
    },
    {
      "ayah_number": 7,
      "text": "وَحِفْظاٗ مِّن كُلِّ شَيْطَٰنٖ مَّارِدٖۖ"
    },
    {
      "ayah_number": 8,
      "text": "لَّا يَسْمَعُونَ إِلَى اَ۬لْمَلَإِ اِ۬لَاعْل۪ىٰ وَيُقْذَفُونَ مِن كُلِّ جَانِبٖۖ"
    },
    {
      "ayah_number": 9,
      "text": "دُحُوراٗۖ وَلَهُمْ عَذَابٞ وَاصِبٌ"
    },
    {
      "ayah_number": 10,
      "text": "اِلَّا مَنْ خَطِفَ اَ۬لْخَطْفَةَ فَأَتْبَعَهُۥ شِهَابٞ ثَاقِبٞۖ"
    },
    {
      "ayah_number": 11,
      "text": "فَاسْتَفْتِهِمُۥٓ أَهُمُۥٓ أَشَدُّ خَلْقاً اَم مَّنْ خَلَقْنَآۖ إِنَّا خَلَقْنَٰهُم مِّن طِينٖ لَّٰزِبِۢۖ"
    },
    {
      "ayah_number": 12,
      "text": "بَلْ عَجِبْتَ وَيَسْخَرُونَۖ"
    },
    {
      "ayah_number": 13,
      "text": "وَإِذَا ذُكِّرُواْ لَا يَذْكُرُونَۖ"
    },
    {
      "ayah_number": 14,
      "text": "وَإِذَا رَأَوَاْ اٰيَةٗ يَسْتَسْخِرُونَۖ"
    },
    {
      "ayah_number": 15,
      "text": "وَقَالُوٓاْ إِنْ هَٰذَآ إِلَّا سِحْرٞ مُّبِينٌ"
    },
    {
      "ayah_number": 16,
      "text": "اَ۟ذَا مِتْنَا وَكُنَّا تُرَاباٗ وَعِظَٰماً اِنَّا لَمَبْعُوثُونَ"
    },
    {
      "ayah_number": 17,
      "text": "أَوَءَابَآؤُنَا اَ۬لَاوَّلُونَۖ"
    },
    {
      "ayah_number": 18,
      "text": "قُلْ نَعَمْ وَأَنتُمْ دَٰخِرُونَۖ"
    },
    {
      "ayah_number": 19,
      "text": "فَإِنَّمَا هِيَ زَجْرَةٞ وَٰحِدَةٞ فَإِذَا هُمْ يَنظُرُونَۖ"
    },
    {
      "ayah_number": 20,
      "text": "وَقَالُواْ يَٰوَيْلَنَا هَٰذَا يَوْمُ اُ۬لدِّينِۖ"
    },
    {
      "ayah_number": 21,
      "text": "هَٰذَا يَوْمُ اُ۬لْفَصْلِ اِ۬لذِے كُنتُم بِهِۦ تُكَذِّبُونَۖ"
    },
    {
      "ayah_number": 22,
      "text": "اَ۟حْشُرُواْ اُ۬لذِينَ ظَلَمُواْ وَأَزْوَٰجَهُمْ وَمَا كَانُواْ يَعْبُدُونَ"
    },
    {
      "ayah_number": 23,
      "text": "مِن دُونِ اِ۬للَّهِ فَاهْدُوهُمُۥٓ إِلَىٰ صِرَٰطِ اِ۬لْجَحِيمِۖ"
    },
    {
      "ayah_number": 24,
      "text": "وَقِفُوهُمُۥٓ إِنَّهُم مَّسْـُٔولُونَۖ"
    },
    {
      "ayah_number": 25,
      "text": "مَا لَكُمْ لَا تَنَاصَرُونَۖ"
    },
    {
      "ayah_number": 26,
      "text": "بَلْ هُمُ اُ۬لْيَوْمَ مُسْتَسْلِمُونَۖ"
    },
    {
      "ayah_number": 27,
      "text": "وَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٖ يَتَسَآءَلُونَۖ"
    },
    {
      "ayah_number": 28,
      "text": "قَالُوٓاْ إِنَّكُمْ كُنتُمْ تَاتُونَنَا عَنِ اِ۬لْيَمِينِۖ"
    },
    {
      "ayah_number": 29,
      "text": "قَالُواْ بَل لَّمْ تَكُونُواْ مُومِنِينَۖ"
    },
    {
      "ayah_number": 30,
      "text": "وَمَا كَانَ لَنَا عَلَيْكُم مِّن سُلْطَٰنِۢ بَلْ كُنتُمْ قَوْماٗ طَٰغِينَۖ"
    },
    {
      "ayah_number": 31,
      "text": "فَحَقَّ عَلَيْنَا قَوْلُ رَبِّنَآ إِنَّا لَذَآئِقُونَ"
    },
    {
      "ayah_number": 32,
      "text": "فَأَغْوَيْنَٰكُمُۥٓ إِنَّا كُنَّا غَٰوِينَۖ"
    },
    {
      "ayah_number": 33,
      "text": "فَإِنَّهُمْ يَوْمَئِذٖ فِے اِ۬لْعَذَابِ مُشْتَرِكُونَۖ"
    },
    {
      "ayah_number": 34,
      "text": "إِنَّا كَذَٰلِكَ نَفْعَلُ بِالْمُجْرِمِينَۖ"
    },
    {
      "ayah_number": 35,
      "text": "إِنَّهُمْ كَانُوٓاْ إِذَا قِيلَ لَهُمْ لَآ إِلَٰهَ إِلَّا اَ۬للَّهُ يَسْتَكْبِرُونَ"
    },
    {
      "ayah_number": 36,
      "text": "وَيَقُولُونَ أَئِنَّا لَتَارِكُوٓاْ ءَالِهَتِنَا لِشَاعِرٖ مَّجْنُونِۢۖ"
    },
    {
      "ayah_number": 37,
      "text": "بَلْ جَآءَ بِالْحَقِّ وَصَدَّقَ اَ۬لْمُرْسَلِينَۖ"
    },
    {
      "ayah_number": 38,
      "text": "إِنَّكُمْ لَذَآئِقُواْ اُ۬لْعَذَابِ اِ۬لَالِيمِۖ"
    },
    {
      "ayah_number": 39,
      "text": "وَمَا تُجْزَوْنَ إِلَّا مَا كُنتُمْ تَعْمَلُونَ"
    },
    {
      "ayah_number": 40,
      "text": "إِلَّا عِبَادَ اَ۬للَّهِ اِ۬لْمُخْلَصِينَۖ"
    },
    {
      "ayah_number": 41,
      "text": "أُوْلَٰٓئِكَ لَهُمْ رِزْقٞ مَّعْلُومٞ"
    },
    {
      "ayah_number": 42,
      "text": "فَوَٰكِهُۖ وَهُم مُّكْرَمُونَ"
    },
    {
      "ayah_number": 43,
      "text": "فِے جَنَّٰتِ اِ۬لنَّعِيمِ"
    },
    {
      "ayah_number": 44,
      "text": "عَلَىٰ سُرُرٖ مُّتَقَٰبِلِينَۖ"
    },
    {
      "ayah_number": 45,
      "text": "يُطَافُ عَلَيْهِم بِكَأْسٖ مِّن مَّعِينِۢ"
    },
    {
      "ayah_number": 46,
      "text": "بَيْضَآءَ لَذَّةٖ لِّلشَّٰرِبِينَ"
    },
    {
      "ayah_number": 47,
      "text": "لَا فِيهَا غَوْلٞ وَلَا هُمْ عَنْهَا يُنزَفُونَۖ"
    },
    {
      "ayah_number": 48,
      "text": "وَعِندَهُمْ قَٰصِرَٰتُ اُ۬لطَّرْفِ عِينٞ"
    },
    {
      "ayah_number": 49,
      "text": "كَأَنَّهُنَّ بَيْضٞ مَّكْنُونٞۖ"
    },
    {
      "ayah_number": 50,
      "text": "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٖ يَتَسَآءَلُونَۖ"
    },
    {
      "ayah_number": 51,
      "text": "قَالَ قَآئِلٞ مِّنْهُمُۥٓ إِنِّے كَانَ لِے قَرِينٞ"
    },
    {
      "ayah_number": 52,
      "text": "يَقُولُ أَ۟نَّكَ لَمِنَ اَ۬لْمُصَدِّقِينَ"
    },
    {
      "ayah_number": 53,
      "text": "أَ۟ذَا مِتْنَا وَكُنَّا تُرَاباٗ وَعِظَٰماً اِنَّا لَمَدِينُونَۖ"
    },
    {
      "ayah_number": 54,
      "text": "قَالَ هَلَ اَنتُم مُّطَّلِعُونَ"
    },
    {
      "ayah_number": 55,
      "text": "فَاطَّلَعَ فَر۪ء۪اهُ فِے سَوَآءِ اِ۬لْجَحِيمِۖ"
    },
    {
      "ayah_number": 56,
      "text": "قَالَ تَاللَّهِ إِن كِدتَّ لَتُرْدِينِۦ"
    },
    {
      "ayah_number": 57,
      "text": "وَلَوْلَا نِعْمَةُ رَبِّے لَكُنتُ مِنَ اَ۬لْمُحْضَرِينَۖ"
    },
    {
      "ayah_number": 58,
      "text": "أَفَمَا نَحْنُ بِمَيِّتِينَ"
    },
    {
      "ayah_number": 59,
      "text": "إِلَّا مَوْتَتَنَا اَ۬لُاول۪ىٰ وَمَا نَحْنُ بِمُعَذَّبِينَۖ"
    },
    {
      "ayah_number": 60,
      "text": "إِنَّ هَٰذَا لَهُوَ اَ۬لْفَوْزُ اُ۬لْعَظِيمُۖ"
    },
    {
      "ayah_number": 61,
      "text": "لِمِثْلِ هَٰذَا فَلْيَعْمَلِ اِ۬لْعَٰمِلُونَۖ"
    },
    {
      "ayah_number": 62,
      "text": "أَذَٰلِكَ خَيْرٞ نُّزُلاً اَمْ شَجَرَةُ اُ۬لزَّقُّومِۖ"
    },
    {
      "ayah_number": 63,
      "text": "إِنَّا جَعَلْنَٰهَا فِتْنَةٗ لِّلظَّٰلِمِينَۖ"
    },
    {
      "ayah_number": 64,
      "text": "إِنَّهَا شَجَرَةٞ تَخْرُجُ فِےٓ أَصْلِ اِ۬لْجَحِيمِ"
    },
    {
      "ayah_number": 65,
      "text": "طَلْعُهَا كَأَنَّهُۥ رُءُوسُ اُ۬لشَّيَٰطِينِۖ"
    },
    {
      "ayah_number": 66,
      "text": "فَإِنَّهُمْ لَأٓكِلُونَ مِنْهَا فَمَالِـُٔونَ مِنْهَا اَ۬لْبُطُونَ"
    },
    {
      "ayah_number": 67,
      "text": "ثُمَّ إِنَّ لَهُمْ عَلَيْهَا لَشَوْباٗ مِّنْ حَمِيمٖۖ"
    },
    {
      "ayah_number": 68,
      "text": "ثُمَّ إِنَّ مَرْجِعَهُمْ لَإِلَى اَ۬لْجَحِيمِۖ"
    },
    {
      "ayah_number": 69,
      "text": "إِنَّهُمُۥٓ أَلْفَوَاْ اٰبَآءَهُمْ ضَآلِّينَ"
    },
    {
      "ayah_number": 70,
      "text": "فَهُمْ عَلَىٰٓ ءَاثٰ۪رِهِمْ يُهْرَعُونَۖ"
    },
    {
      "ayah_number": 71,
      "text": "وَلَقَد ضَّلَّ قَبْلَهُمُۥٓ أَكْثَرُ اُ۬لَاوَّلِينَۖ"
    },
    {
      "ayah_number": 72,
      "text": "وَلَقَدَ اَرْسَلْنَا فِيهِم مُّنذِرِينَۖ"
    },
    {
      "ayah_number": 73,
      "text": "فَانظُرْ كَيْفَ كَانَ عَٰقِبَةُ اُ۬لْمُنذَرِينَ"
    },
    {
      "ayah_number": 74,
      "text": "إِلَّا عِبَادَ اَ۬للَّهِ اِ۬لْمُخْلَصِينَۖ"
    },
    {
      "ayah_number": 75,
      "text": "وَلَقَدْ نَاد۪يٰنَا نُوحٞۖ فَلَنِعْمَ اَ۬لْمُجِيبُونَۖ"
    },
    {
      "ayah_number": 76,
      "text": "وَنَجَّيْنَٰهُ وَأَهْلَهُۥ مِنَ اَ۬لْكَرْبِ اِ۬لْعَظِيمِۖ"
    },
    {
      "ayah_number": 77,
      "text": "وَجَعَلْنَا ذُرِّيَّتَهُۥ هُمُ اُ۬لْبَاقِينَۖ"
    },
    {
      "ayah_number": 78,
      "text": "وَتَرَكْنَا عَلَيْهِ فِے اِ۬لَاخِرِينَۖ"
    },
    {
      "ayah_number": 79,
      "text": "سَلَٰمٌ عَلَىٰ نُوحٖ فِے اِ۬لْعَٰلَمِينَۖ"
    },
    {
      "ayah_number": 80,
      "text": "إِنَّا كَذَٰلِكَ نَجْزِے اِ۬لْمُحْسِنِينَۖ"
    },
    {
      "ayah_number": 81,
      "text": "إِنَّهُۥ مِنْ عِبَادِنَا اَ۬لْمُومِنِينَۖ"
    },
    {
      "ayah_number": 82,
      "text": "ثُمَّ أَغْرَقْنَا اَ۬لَاخَرِينَۖ"
    },
    {
      "ayah_number": 83,
      "text": "وَإِنَّ مِن شِيعَتِهِۦ لَإِبْرَٰهِيمَ"
    },
    {
      "ayah_number": 84,
      "text": "إِذْ جَآءَ رَبَّهُۥ بِقَلْبٖ سَلِيمٍ"
    },
    {
      "ayah_number": 85,
      "text": "اِذْ قَالَ لِأَبِيهِ وَقَوْمِهِۦ مَاذَا تَعْبُدُونَۖ"
    },
    {
      "ayah_number": 86,
      "text": "أَئِفْكاً اٰلِهَةٗ دُونَ اَ۬للَّهِ تُرِيدُونَۖ"
    },
    {
      "ayah_number": 87,
      "text": "فَمَا ظَنُّكُم بِرَبِّ اِ۬لْعَٰلَمِينَۖ"
    },
    {
      "ayah_number": 88,
      "text": "فَنَظَرَ نَظْرَةٗ فِے اِ۬لنُّجُومِ"
    },
    {
      "ayah_number": 89,
      "text": "فَقَالَ إِنِّے سَقِيمٞ"
    },
    {
      "ayah_number": 90,
      "text": "فَتَوَلَّوْاْ عَنْهُ مُدْبِرِينَۖ"
    },
    {
      "ayah_number": 91,
      "text": "فَرَاغَ إِلَىٰٓ ءَالِهَتِهِمْ فَقَالَ أَلَا تَاكُلُونَ"
    },
    {
      "ayah_number": 92,
      "text": "مَا لَكُمْ لَا تَنطِقُونَۖ"
    },
    {
      "ayah_number": 93,
      "text": "فَرَاغَ عَلَيْهِمْ ضَرْباَۢ بِالْيَمِينِ"
    },
    {
      "ayah_number": 94,
      "text": "فَأَقْبَلُوٓاْ إِلَيْهِ يَزِفُّونَۖ"
    },
    {
      "ayah_number": 95,
      "text": "قَالَ أَتَعْبُدُونَ مَا تَنْحِتُونَ"
    },
    {
      "ayah_number": 96,
      "text": "وَاللَّهُ خَلَقَكُمْ وَمَا تَعْمَلُونَۖ"
    },
    {
      "ayah_number": 97,
      "text": "قَالُواْ اُ۪بْنُواْ لَهُۥ بُنْيَٰناٗ فَأَلْقُوهُ فِے اِ۬لْجَحِيمِ"
    },
    {
      "ayah_number": 98,
      "text": "فَأَرَادُواْ بِهِۦ كَيْداٗ فَجَعَلْنَٰهُمُ اُ۬لَاسْفَلِينَۖ"
    },
    {
      "ayah_number": 99,
      "text": "وَقَالَ إِنِّے ذَاهِبٌ اِلَىٰ رَبِّے سَيَهْدِينِۖ"
    },
    {
      "ayah_number": 100,
      "text": "رَبِّ هَبْ لِے مِنَ اَ۬لصَّٰلِحِينَۖ"
    },
    {
      "ayah_number": 101,
      "text": "فَبَشَّرْنَٰهُ بِغُلَٰمٍ حَلِيمٖۖ"
    },
    {
      "ayah_number": 102,
      "text": "فَلَمَّا بَلَغَ مَعَهُ اُ۬لسَّعْيَ قَالَ يَٰبُنَيِّ إِنِّيَ أَر۪ىٰ فِے اِ۬لْمَنَامِ أَنِّيَ أَذْبَحُكَ فَانظُرْ مَاذَا تَر۪ىٰۖ قَالَ يَٰٓأَبَتِ اِ۪فْعَلْ مَا تُومَرُۖ سَتَجِدُنِيَ إِن شَآءَ اَ۬للَّهُ مِنَ اَ۬لصَّٰبِرِينَۖ"
    },
    {
      "ayah_number": 103,
      "text": "فَلَمَّآ أَسْلَمَا وَتَلَّهُۥ لِلْجَبِينِ"
    },
    {
      "ayah_number": 104,
      "text": "وَنَٰدَيْنَٰهُ أَنْ يَّٰٓإِبْرَٰهِيمُ"
    },
    {
      "ayah_number": 105,
      "text": "قَدْ صَدَّقْتَ اَ۬لرُّءْي۪آۖ إِنَّا كَذَٰلِكَ نَجْزِے اِ۬لْمُحْسِنِينَۖ"
    },
    {
      "ayah_number": 106,
      "text": "إِنَّ هَٰذَا لَهُوَ اَ۬لْبَلَٰٓؤُاْ اُ۬لْمُبِينُۖ"
    },
    {
      "ayah_number": 107,
      "text": "وَفَدَيْنَٰهُ بِذِبْحٍ عَظِيمٖۖ"
    },
    {
      "ayah_number": 108,
      "text": "وَتَرَكْنَا عَلَيْهِ فِے اِ۬لَاخِرِينَۖ"
    },
    {
      "ayah_number": 109,
      "text": "سَلَٰمٌ عَلَىٰٓ إِبْرَٰهِيمَۖ"
    },
    {
      "ayah_number": 110,
      "text": "كَذَٰلِكَ نَجْزِے اِ۬لْمُحْسِنِينَۖ"
    },
    {
      "ayah_number": 111,
      "text": "إِنَّهُۥ مِنْ عِبَادِنَا اَ۬لْمُومِنِينَۖ"
    },
    {
      "ayah_number": 112,
      "text": "وَبَشَّرْنَٰهُ بِإِسْحَٰقَ نَبِيٓـٔاٗ مِّنَ اَ۬لصَّٰلِحِينَ"
    },
    {
      "ayah_number": 113,
      "text": "وَبَٰرَكْنَا عَلَيْهِ وَعَلَىٰٓ إِسْحَٰقَۖ وَمِن ذُرِّيَّتِهِمَا مُحْسِنٞ وَظَالِمٞ لِّنَفْسِهِۦ مُبِينٞۖ"
    },
    {
      "ayah_number": 114,
      "text": "وَلَقَدْ مَنَنَّا عَلَىٰ مُوس۪ىٰ وَهَٰرُونَ"
    },
    {
      "ayah_number": 115,
      "text": "وَنَجَّيْنَٰهُمَا وَقَوْمَهُمَا مِنَ اَ۬لْكَرْبِ اِ۬لْعَظِيمِۖ"
    },
    {
      "ayah_number": 116,
      "text": "وَنَصَرْنَٰهُمْ فَكَانُواْ هُمُ اُ۬لْغَٰلِبِينَۖ"
    },
    {
      "ayah_number": 117,
      "text": "وَءَاتَيْنَٰهُمَا اَ۬لْكِتَٰبَ اَ۬لْمُسْتَبِينَۖ"
    },
    {
      "ayah_number": 118,
      "text": "وَهَدَيْنَٰهُمَا اَ۬لصِّرَٰطَ اَ۬لْمُسْتَقِيمَۖ"
    },
    {
      "ayah_number": 119,
      "text": "وَتَرَكْنَا عَلَيْهِمَا فِے اِ۬لَاخِرِينَۖ"
    },
    {
      "ayah_number": 120,
      "text": "سَلَٰمٌ عَلَىٰ مُوس۪ىٰ وَهَٰرُونَۖ"
    },
    {
      "ayah_number": 121,
      "text": "إِنَّا كَذَٰلِكَ نَجْزِے اِ۬لْمُحْسِنِينَۖ"
    },
    {
      "ayah_number": 122,
      "text": "إِنَّهُمَا مِنْ عِبَادِنَا اَ۬لْمُومِنِينَۖ"
    },
    {
      "ayah_number": 123,
      "text": "وَإِنَّ إِلْيَاسَ لَمِنَ اَ۬لْمُرْسَلِينَ"
    },
    {
      "ayah_number": 124,
      "text": "إِذْ قَالَ لِقَوْمِهِۦٓ أَلَا تَتَّقُونَ"
    },
    {
      "ayah_number": 125,
      "text": "أَتَدْعُونَ بَعْلاٗ وَتَذَرُونَ أَحْسَنَ اَ۬لْخَٰلِقِينَۖ"
    },
    {
      "ayah_number": 126,
      "text": "اَ۬للَّهُ رَبُّكُمْ وَرَبُّ ءَابَآئِكُمُ اُ۬لَاوَّلِينَۖ"
    },
    {
      "ayah_number": 127,
      "text": "فَكَذَّبُوهُ فَإِنَّهُمْ لَمُحْضَرُونَ"
    },
    {
      "ayah_number": 128,
      "text": "إِلَّا عِبَادَ اَ۬للَّهِ اِ۬لْمُخْلَصِينَۖ"
    },
    {
      "ayah_number": 129,
      "text": "وَتَرَكْنَا عَلَيْهِ فِے اِ۬لَاخِرِينَۖ"
    },
    {
      "ayah_number": 130,
      "text": "سَلَٰمٌ عَلَىٰٓ ءَالِ يَاسِينَۖ"
    },
    {
      "ayah_number": 131,
      "text": "إِنَّا كَذَٰلِكَ نَجْزِے اِ۬لْمُحْسِنِينَۖ"
    },
    {
      "ayah_number": 132,
      "text": "إِنَّهُۥ مِنْ عِبَادِنَا اَ۬لْمُومِنِينَۖ"
    },
    {
      "ayah_number": 133,
      "text": "وَإِنَّ لُوطاٗ لَّمِنَ اَ۬لْمُرْسَلِينَ"
    },
    {
      "ayah_number": 134,
      "text": "إِذْ نَجَّيْنَٰهُ وَأَهْلَهُۥٓ أَجْمَعِينَ"
    },
    {
      "ayah_number": 135,
      "text": "إِلَّا عَجُوزاٗ فِے اِ۬لْغَٰبِرِينَ"
    },
    {
      "ayah_number": 136,
      "text": "ثُمَّ دَمَّرْنَا اَ۬لَاخَرِينَۖ"
    },
    {
      "ayah_number": 137,
      "text": "وَإِنَّكُمْ لَتَمُرُّونَ عَلَيْهِم مُّصْبِحِينَ"
    },
    {
      "ayah_number": 138,
      "text": "وَبِاليْلِۖ أَفَلَا تَعْقِلُونَۖ"
    },
    {
      "ayah_number": 139,
      "text": "وَإِنَّ يُونُسَ لَمِنَ اَ۬لْمُرْسَلِينَ"
    },
    {
      "ayah_number": 140,
      "text": "إِذَ اَبَقَ إِلَى اَ۬لْفُلْكِ اِ۬لْمَشْحُونِ"
    },
    {
      "ayah_number": 141,
      "text": "فَسَاهَمَ فَكَانَ مِنَ اَ۬لْمُدْحَضِينَ"
    },
    {
      "ayah_number": 142,
      "text": "فَالْتَقَمَهُ اُ۬لْحُوتُ وَهُوَ مُلِيمٞۖ"
    },
    {
      "ayah_number": 143,
      "text": "فَلَوْلَآ أَنَّهُۥ كَانَ مِنَ اَ۬لْمُسَبِّحِينَ"
    },
    {
      "ayah_number": 144,
      "text": "لَلَبِثَ فِے بَطْنِهِۦٓ إِلَىٰ يَوْمِ يُبْعَثُونَۖ"
    },
    {
      "ayah_number": 145,
      "text": "فَنَبَذْنَٰهُ بِالْعَرَآءِ وَهُوَ سَقِيمٞۖ"
    },
    {
      "ayah_number": 146,
      "text": "وَأَنۢبَتْنَا عَلَيْهِ شَجَرَةٗ مِّنْ يَّقْطِينٖۖ"
    },
    {
      "ayah_number": 147,
      "text": "وَأَرْسَلْنَٰهُ إِلَىٰ مِاْئَةِ أَلْفٍ اَوْ يَزِيدُونَۖ"
    },
    {
      "ayah_number": 148,
      "text": "فَـَٔامَنُواْ فَمَتَّعْنَٰهُمُۥٓ إِلَىٰ حِينٖۖ"
    },
    {
      "ayah_number": 149,
      "text": "فَاسْتَفْتِهِمُۥٓ أَلِرَبِّكَ اَ۬لْبَنَاتُ وَلَهُمُ اُ۬لْبَنُونَۖ"
    },
    {
      "ayah_number": 150,
      "text": "أَمْ خَلَقْنَا اَ۬لْمَلَٰٓئِكَةَ إِنَٰثاٗ وَهُمْ شَٰهِدُونَۖ"
    },
    {
      "ayah_number": 151,
      "text": "أَلَآ إِنَّهُم مِّنِ اِفْكِهِمْ لَيَقُولُونَ"
    },
    {
      "ayah_number": 152,
      "text": "وَلَدَ اَ۬للَّهُ وَإِنَّهُمْ لَكَٰذِبُونَۖ"
    },
    {
      "ayah_number": 153,
      "text": "أَصْطَفَى اَ۬لْبَنَاتِ عَلَى اَ۬لْبَنِينَۖ"
    },
    {
      "ayah_number": 154,
      "text": "مَا لَكُمْۖ كَيْفَ تَحْكُمُونَۖ"
    },
    {
      "ayah_number": 155,
      "text": "أَفَلَا تَذَّكَّرُونَۖ"
    },
    {
      "ayah_number": 156,
      "text": "أَمْ لَكُمْ سُلْطَٰنٞ مُّبِينٞ"
    },
    {
      "ayah_number": 157,
      "text": "فَاتُواْ بِكِتَٰبِكُمُۥٓ إِن كُنتُمْ صَٰدِقِينَۖ"
    },
    {
      "ayah_number": 158,
      "text": "وَجَعَلُواْ بَيْنَهُۥ وَبَيْنَ اَ۬لْجِنَّةِ نَسَباٗۖ وَلَقَدْ عَلِمَتِ اِ۬لْجِنَّةُ إِنَّهُمْ لَمُحْضَرُونَۖ"
    },
    {
      "ayah_number": 159,
      "text": "سُبْحَٰنَ اَ۬للَّهِ عَمَّا يَصِفُونَۖ"
    },
    {
      "ayah_number": 160,
      "text": "إِلَّا عِبَادَ اَ۬للَّهِ اِ۬لْمُخْلَصِينَۖ"
    },
    {
      "ayah_number": 161,
      "text": "فَإِنَّكُمْ وَمَا تَعْبُدُونَ"
    },
    {
      "ayah_number": 162,
      "text": "مَآ أَنتُمْ عَلَيْهِ بِفَٰتِنِينَ"
    },
    {
      "ayah_number": 163,
      "text": "إِلَّا مَنْ هُوَ صَالِ اِ۬لْجَحِيمِۖ"
    },
    {
      "ayah_number": 164,
      "text": "وَمَا مِنَّآ إِلَّا لَهُۥ مَقَامٞ مَّعْلُومٞۖ"
    },
    {
      "ayah_number": 165,
      "text": "وَإِنَّا لَنَحْنُ اُ۬لصَّآفُّونَ"
    },
    {
      "ayah_number": 166,
      "text": "وَإِنَّا لَنَحْنُ اُ۬لْمُسَبِّحُونَۖ"
    },
    {
      "ayah_number": 167,
      "text": "وَإِن كَانُواْ لَيَقُولُونَ"
    },
    {
      "ayah_number": 168,
      "text": "لَوَ اَنَّ عِندَنَا ذِكْراٗ مِّنَ اَ۬لَاوَّلِينَ"
    },
    {
      "ayah_number": 169,
      "text": "لَكُنَّا عِبَادَ اَ۬للَّهِ اِ۬لْمُخْلَصِينَۖ"
    },
    {
      "ayah_number": 170,
      "text": "فَكَفَرُواْ بِهِۦۖ فَسَوْفَ يَعْلَمُونَۖ"
    },
    {
      "ayah_number": 171,
      "text": "وَلَقَدْ سَبَقَتْ كَلِمَتُنَا لِعِبَادِنَا اَ۬لْمُرْسَلِينَ"
    },
    {
      "ayah_number": 172,
      "text": "إِنَّهُمْ لَهُمُ اُ۬لْمَنصُورُونَ"
    },
    {
      "ayah_number": 173,
      "text": "وَإِنَّ جُندَنَا لَهُمُ اُ۬لْغَٰلِبُونَۖ"
    },
    {
      "ayah_number": 174,
      "text": "فَتَوَلَّ عَنْهُمْ حَتَّىٰ حِينٖۖ"
    },
    {
      "ayah_number": 175,
      "text": "وَأَبْصِرْهُمْ فَسَوْفَ يُبْصِرُونَۖ"
    },
    {
      "ayah_number": 176,
      "text": "أَفَبِعَذَابِنَا يَسْتَعْجِلُونَۖ"
    },
    {
      "ayah_number": 177,
      "text": "فَإِذَا نَزَلَ بِسَاحَتِهِمْ فَسَآءَ صَبَاحُ اُ۬لْمُنذَرِينَۖ"
    },
    {
      "ayah_number": 178,
      "text": "وَتَوَلَّ عَنْهُمْ حَتَّىٰ حِينٖۖ"
    },
    {
      "ayah_number": 179,
      "text": "وَأَبْصِرْ فَسَوْفَ يُبْصِرُونَۖ"
    },
    {
      "ayah_number": 180,
      "text": "سُبْحَٰنَ رَبِّكَ رَبِّ اِ۬لْعِزَّةِ عَمَّا يَصِفُونَۖ"
    },
    {
      "ayah_number": 181,
      "text": "وَسَلَٰمٌ عَلَى اَ۬لْمُرْسَلِينَۖ"
    },
    {
      "ayah_number": 182,
      "text": "وَالْحَمْدُ لِلهِ رَبِّ اِ۬لْعَٰلَمِينَۖ"
    }
  ]
}