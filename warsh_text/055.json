{
  "surah_number": 55,
  "name": "اُ۬لرَّحْمَٰن",
  "number_of_ayahs": 77,
  "ayahs": [
    {
      "ayah_number": 1,
      "text": "اِ۬لرَّحْمَٰنُ عَلَّمَ اَ۬لْقُرْءَانَۖ"
    },
    {
      "ayah_number": 2,
      "text": "خَلَقَ اَ۬لِانسَٰنَ عَلَّمَهُ اُ۬لْبَيَانَۖ"
    },
    {
      "ayah_number": 3,
      "text": "اَ۬لشَّمْسُ وَالْقَمَرُ بِحُسْبَانٖۖ"
    },
    {
      "ayah_number": 4,
      "text": "وَالنَّجْمُ وَالشَّجَرُ يَسْجُدَٰنِۖ"
    },
    {
      "ayah_number": 5,
      "text": "وَالسَّمَآءَ رَفَعَهَا وَوَضَعَ اَ۬لْمِيزَانَ"
    },
    {
      "ayah_number": 6,
      "text": "أَلَّا تَطْغَوْاْ فِے اِ۬لْمِيزَانِ"
    },
    {
      "ayah_number": 7,
      "text": "وَأَقِيمُواْ اُ۬لْوَزْنَ بِالْقِسْطِ وَلَا تُخْسِرُواْ اُ۬لْمِيزَانَۖ"
    },
    {
      "ayah_number": 8,
      "text": "وَالَارْضَ وَضَعَهَا لِلَانَامِ"
    },
    {
      "ayah_number": 9,
      "text": "فِيهَا فَٰكِهَةٞ وَالنَّخْلُ ذَاتُ اُ۬لَاكْمَامِ"
    },
    {
      "ayah_number": 10,
      "text": "وَالْحَبُّ ذُو اُ۬لْعَصْفِ وَالرَّيْحَانُۖ"
    },
    {
      "ayah_number": 11,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 12,
      "text": "خَلَقَ اَ۬لِانسَٰنَ مِن صَلْصَٰلٖ كَالْفَخّ۪ارِ"
    },
    {
      "ayah_number": 13,
      "text": "وَخَلَقَ اَ۬لْجَآنَّ مِن مَّارِجٖ مِّن نّ۪ارٖۖ"
    },
    {
      "ayah_number": 14,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 15,
      "text": "رَبُّ اُ۬لْمَشْرِقَيْنِ وَرَبُّ اُ۬لْمَغْرِبَيْنِۖ"
    },
    {
      "ayah_number": 16,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 17,
      "text": "مَرَجَ اَ۬لْبَحْرَيْنِ يَلْتَقِيَٰنِ"
    },
    {
      "ayah_number": 18,
      "text": "بَيْنَهُمَا بَرْزَخٞ لَّا يَبْغِيَٰنِۖ"
    },
    {
      "ayah_number": 19,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 20,
      "text": "يُخْرَجُ مِنْهُمَا اَ۬للُّؤْلُؤُاْ وَالْمَرْجَانُۖ"
    },
    {
      "ayah_number": 21,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 22,
      "text": "وَلَهُ اُ۬لْجَوَارِ اِ۬لْمُنشَأَٰتُ فِے اِ۬لْبَحْرِ كَالَاعْلَٰمِۖ"
    },
    {
      "ayah_number": 23,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 24,
      "text": "كُلُّ مَنْ عَلَيْهَا فَانٖ"
    },
    {
      "ayah_number": 25,
      "text": "وَيَبْق۪ىٰ وَجْهُ رَبِّكَ ذُو اُ۬لْجَلَٰلِ وَالِاكْرَامِۖ"
    },
    {
      "ayah_number": 26,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 27,
      "text": "يَسْـَٔلُهُۥ مَن فِے اِ۬لسَّمَٰوَٰتِ وَالَارْضِ كُلَّ يَوْمٍ هُوَ فِے شَأْنٖۖ"
    },
    {
      "ayah_number": 28,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 29,
      "text": "سَنَفْرُغُ لَكُمُۥٓ أَيُّهَ اَ۬لثَّقَلَٰنِۖ"
    },
    {
      "ayah_number": 30,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 31,
      "text": "يَٰمَعْشَرَ اَ۬لْجِنِّ وَالِانسِ إِنِ اِ۪سْتَطَعْتُمُۥٓ أَن تَنفُذُواْ مِنَ اَقْط۪ارِ اِ۬لسَّمَٰوَٰتِ وَالَارْضِ فَانفُذُواْۖ لَا تَنفُذُونَ إِلَّا بِسُلْطَٰنٖۖ"
    },
    {
      "ayah_number": 32,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 33,
      "text": "يُرْسَلُ عَلَيْكُمَا شُوَاظٞ مِّن نّ۪ارٖ"
    },
    {
      "ayah_number": 34,
      "text": "وَنُحَاسٞ فَلَا تَنتَصِرَٰنِۖ"
    },
    {
      "ayah_number": 35,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 36,
      "text": "فَإِذَا اَ۪نشَقَّتِ اِ۬لسَّمَآءُ فَكَانَتْ وَرْدَةٗ كَالدِّهَانِ"
    },
    {
      "ayah_number": 37,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "ayah_number": 38,
      "text": "فَيَوْمَئِذٖ لَّا يُسْـَٔلُ عَن ذَنۢبِهِۦٓ إِنسٞ وَلَا جَآنّٞۖ"
    },
    {
      "ayah_number": 39,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 40,
      "text": "يُعْرَفُ اُ۬لْمُجْرِمُونَ بِسِيمٰ۪هُمْ فَيُوخَذُ بِالنَّوَٰصِے وَالَاقْدَامِۖ"
    },
    {
      "ayah_number": 41,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 42,
      "text": "هَٰذِهِۦ جَهَنَّمُ اُ۬لتِے يُكَذِّبُ بِهَا اَ۬لْمُجْرِمُونَ"
    },
    {
      "ayah_number": 43,
      "text": "يَطُوفُونَ بَيْنَهَا وَبَيْنَ حَمِيمٍ اٰنٖۖ"
    },
    {
      "ayah_number": 44,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 45,
      "text": "وَلِمَنْ خَافَ مَقَامَ رَبِّهِۦ جَنَّتَٰنِ"
    },
    {
      "ayah_number": 46,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "ayah_number": 47,
      "text": "ذَوَاتَآ أَفْنَانٖۖ"
    },
    {
      "ayah_number": 48,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 49,
      "text": "فِيهِمَا عَيْنَٰنِ تَجْرِيَٰنِۖ"
    },
    {
      "ayah_number": 50,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 51,
      "text": "فِيهِمَا مِن كُلِّ فَٰكِهَةٖ زَوْجَٰنِۖ"
    },
    {
      "ayah_number": 52,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 53,
      "text": "مُتَّكِـِٕينَ عَلَىٰ فُرُشِۢ بَطَآئِنُهَا مِنِ اِسْتَبْرَقٖ وَجَنَا اَ۬لْجَنَّتَيْنِ دَانٖۖ"
    },
    {
      "ayah_number": 54,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 55,
      "text": "فِيهِنَّ قَٰصِرَٰتُ اُ۬لطَّرْفِ لَمْ يَطْمِثْهُنَّ إِنسٞ قَبْلَهُمْ وَلَا جَآنّٞۖ"
    },
    {
      "ayah_number": 56,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 57,
      "text": "كَأَنَّهُنَّ اَ۬لْيَاقُوتُ وَالْمَرْجَانُۖ"
    },
    {
      "ayah_number": 58,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 59,
      "text": "هَلْ جَزَآءُ اُ۬لِاحْسَٰنِ إِلَّا اَ۬لِاحْسَٰنُۖ"
    },
    {
      "ayah_number": 60,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 61,
      "text": "وَمِن دُونِهِمَا جَنَّتَٰنِ"
    },
    {
      "ayah_number": 62,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "ayah_number": 63,
      "text": "مُدْهَآمَّتَٰنِۖ"
    },
    {
      "ayah_number": 64,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 65,
      "text": "فِيهِمَا عَيْنَٰنِ نَضَّاخَتَٰنِۖ"
    },
    {
      "ayah_number": 66,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 67,
      "text": "فِيهِمَا فَٰكِهَةٞ وَنَخْلٞ وَرُمَّانٞۖ"
    },
    {
      "ayah_number": 68,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 69,
      "text": "فِيهِنَّ خَيْرَٰتٌ حِسَانٞۖ"
    },
    {
      "ayah_number": 70,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 71,
      "text": "حُورٞ مَّقْصُورَٰتٞ فِے اِ۬لْخِيَامِۖ"
    },
    {
      "ayah_number": 72,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 73,
      "text": "لَمْ يَطْمِثْهُنَّ إِنسٞ قَبْلَهُمْ وَلَا جَآنّٞۖ"
    },
    {
      "ayah_number": 74,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 75,
      "text": "مُتَّكِـِٕينَ عَلَىٰ رَفْرَفٍ خُضْرٖ وَعَبْقَرِيٍّ حِسَانٖۖ"
    },
    {
      "ayah_number": 76,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ"
    },
    {
      "ayah_number": 77,
      "text": "تَبَٰرَكَ اَ۪سْمُ رَبِّكَ ذِے اِ۬لْجَلَٰلِ وَالِاكْرَامِۖ"
    }
  ]
}