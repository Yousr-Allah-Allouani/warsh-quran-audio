  "ayahs": [
    {
      "ayah_number": 1,
      "text": "وَالْعَصْرِ إِنَّ اَ۬لِانسَٰنَ لَفِے خُسْرٍ"
    },
    {
      "ayah_number": 2,
      "text": "اِلَّا اَ۬لذِينَ ءَامَنُواْ وَعَمِلُواْ اُ۬لصَّٰلِحَٰتِ وَتَوَاصَوْاْ بِالْحَقِّ"
    },
    {
      "ayah_number": 3,
      "text": "وَتَوَاصَوْاْ بِالصَّبْرِۖ"
