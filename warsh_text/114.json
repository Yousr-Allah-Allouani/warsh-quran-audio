{
  "surah_number": 114,
  "name": "اُ۬لنَّاسِ",
  "number_of_ayahs": 6,
  "ayahs": [
    {
      "ayah_number": 1,
      "text": "قُلَ اَعُوذُ بِرَبِّ اِ۬لنَّاسِ"
    },
    {
      "ayah_number": 2,
      "text": "مَلِكِ اِ۬لنَّاسِ"
    },
    {
      "ayah_number": 3,
      "text": "إِلَٰهِ اِ۬لنَّاسِ"
    },
    {
      "ayah_number": 4,
      "text": "مِن شَرِّ اِ۬لْوَسْوَاسِ اِ۬لْخَنَّاسِ"
    },
    {
      "ayah_number": 5,
      "text": "اِ۬لذِے يُوَسْوِسُ فِے صُدُورِ اِ۬لنَّاسِ"
    },
    {
      "ayah_number": 6,
      "text": "مِنَ اَ۬لْجِنَّةِ وَالنَّاسِۖ"
    }
  ]
}