{
  "surah_number": 87,
  "name": "اُ۬لَاعْلَي",
  "number_of_ayahs": 19,
  "ayahs": [
    {
      "ayah_number": 1,
      "text": "سَبِّحِ اِ۪سْمَ رَبِّكَ اَ۬لَاعْلَي"
    },
    {
      "ayah_number": 2,
      "text": "اَ۬لذِے خَلَقَ فَسَوّ۪ىٰ"
    },
    {
      "ayah_number": 3,
      "text": "وَالذِے قَدَّرَ فَهَد۪ىٰ"
    },
    {
      "ayah_number": 4,
      "text": "وَالذِےٓ أَخْرَجَ اَ۬لْمَرْع۪ىٰ"
    },
    {
      "ayah_number": 5,
      "text": "فَجَعَلَهُۥ غُثَآءً اَحْو۪ىٰۖ"
    },
    {
      "ayah_number": 6,
      "text": "سَنُقْرِئُكَ فَلَا تَنس۪ىٰٓ"
    },
    {
      "ayah_number": 7,
      "text": "إِلَّا مَا شَآءَ اَ۬للَّهُۖ إِنَّهُۥ يَعْلَمُ اُ۬لْجَهْرَ وَمَا يَخْف۪ىٰۖ"
    },
    {
      "ayah_number": 8,
      "text": "وَنُيَسِّرُكَ لِلْيُسْر۪ىٰۖ"
    },
    {
      "ayah_number": 9,
      "text": "فَذَكِّرِ اِن نَّفَعَتِ اِ۬لذِّكْر۪ىٰۖ"
    },
    {
      "ayah_number": 10,
      "text": "سَيَذَّكَّرُ مَنْ يَّخْش۪ىٰ"
    },
    {
      "ayah_number": 11,
      "text": "وَيَتَجَنَّبُهَا اَ۬لَاشْقَي"
    },
    {
      "ayah_number": 12,
      "text": "اَ۬لذِے يَصْلَى اَ۬لنَّارَ اَ۬لْكُبْر۪ىٰ"
    },
    {
      "ayah_number": 13,
      "text": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْي۪ىٰۖ"
    },
    {
      "ayah_number": 14,
      "text": "قَدَ اَفْلَحَ مَن تَزَكّ۪ىٰ"
    },
    {
      "ayah_number": 15,
      "text": "وَذَكَرَ اَ۪سْمَ رَبِّهِۦ فَصَلّ۪ىٰۖ"
    },
    {
      "ayah_number": 16,
      "text": "بَلْ تُوثِرُونَ اَ۬لْحَيَوٰةَ اَ۬لدُّنْي۪ا"
    },
    {
      "ayah_number": 17,
      "text": "وَالَاخِرَةُ خَيْرٞ وَأَبْق۪ىٰٓۖ"
    },
    {
      "ayah_number": 18,
      "text": "إِنَّ هَٰذَا لَفِے اِ۬لصُّحُفِ اِ۬لُاول۪ىٰ"
    },
    {
      "ayah_number": 19,
      "text": "صُحُفِ إِبْرَٰهِيمَ وَمُوس۪ىٰۖ"
    }
  ]
}