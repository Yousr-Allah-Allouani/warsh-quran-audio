  "warsh_ayahs": [
    {
      "ayah_number": 1,
      "text": "اَلْه۪يٰكُمُ اُ۬لتَّكَاثُرُ"
    },
    {
      "ayah_number": 2,
      "text": "حَتَّىٰ زُرْتُمُ اُ۬لْمَقَابِرَۖ"
    },
    {
      "ayah_number": 3,
      "text": "كَلَّا سَوْفَ تَعْلَمُونَ"
    },
    {
      "ayah_number": 4,
      "text": "ثُمَّ كَلَّا سَوْفَ تَعْلَمُونَۖ"
    },
    {
      "ayah_number": 5,
      "text": "كَلَّا لَوْ تَعْلَمُونَ عِلْمَ اَ۬لْيَقِينِۖ"
    },
    {
      "ayah_number": 6,
      "text": "لَتَرَوُنَّ اَ۬لْجَحِيمَ"
    },
    {
      "ayah_number": 7,
      "text": "ثُمَّ لَتَرَوُنَّهَا عَيْنَ اَ۬لْيَقِينِ"
    },
    {
      "ayah_number": 8,
      "text": "ثُمَّ لَتُسْـَٔلُنَّ يَوْمَئِذٍ عَنِ اِ۬لنَّعِيمِۖ"
    }
  ]
