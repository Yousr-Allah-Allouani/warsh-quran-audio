{
  "surah_number": 86,
  "name": "اُ۬لطَّارِقِ",
  "number_of_ayahs": 17,
  "ayahs": [
    {
      "ayah_number": 1,
      "text": "وَالسَّمَآءِ وَالطَّارِقِ"
    },
    {
      "ayah_number": 2,
      "text": "وَمَآ أَدْر۪يٰكَ مَا اَ۬لطَّارِقُ"
    },
    {
      "ayah_number": 3,
      "text": "اُ۬لنَّجْمُ اُ۬لثَّاقِبُ"
    },
    {
      "ayah_number": 4,
      "text": "إِن كُلُّ نَفْسٖ لَّمَا عَلَيْهَا حَافِظٞۖ"
    },
    {
      "ayah_number": 5,
      "text": "فَلْيَنظُرِ اِ۬لِانسَٰنُ مِمَّ خُلِقَۖ"
    },
    {
      "ayah_number": 6,
      "text": "خُلِقَ مِن مَّآءٖ دَافِقٖ"
    },
    {
      "ayah_number": 7,
      "text": "يَخْرُجُ مِنۢ بَيْنِ اِ۬لصُّلْبِ وَالتَّرَآئِبِۖ"
    },
    {
      "ayah_number": 8,
      "text": "إِنَّهُۥ عَلَىٰ رَجْعِهِۦ لَقَادِرٞ"
    },
    {
      "ayah_number": 9,
      "text": "يَوْمَ تُبْلَى اَ۬لسَّرَآئِرُ"
    },
    {
      "ayah_number": 10,
      "text": "فَمَا لَهُۥ مِن قُوَّةٖ وَلَا نَاصِرٖۖ"
    },
    {
      "ayah_number": 11,
      "text": "وَالسَّمَآءِ ذَاتِ اِ۬لرَّجْعِ"
    },
    {
      "ayah_number": 12,
      "text": "وَالَارْضِ ذَاتِ اِ۬لصَّدْعِ"
    },
    {
      "ayah_number": 13,
      "text": "إِنَّهُۥ لَقَوْلٞ فَصْلٞ"
    },
    {
      "ayah_number": 14,
      "text": "وَمَا هُوَ بِالْهَزْلِۖ"
    },
    {
      "ayah_number": 15,
      "text": "إِنَّهُمْ يَكِيدُونَ كَيْداٗ"
    },
    {
      "ayah_number": 16,
      "text": "وَأَكِيدُ كَيْداٗۖ"
    },
    {
      "ayah_number": 17,
      "text": "فَمَهِّلِ اِ۬لْكٰ۪فِرِينَ أَمْهِلْهُمْ رُوَيْداٗۖ"
    }
  ]
}