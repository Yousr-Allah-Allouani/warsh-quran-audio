{
  "surah_number": 113,
  "name": "اُ۬لْفَلَقِ",
  "number_of_ayahs": 5,
  "ayahs": [
    {
      "ayah_number": 1,
      "text": "قُلَ اَعُوذُ بِرَبِّ اِ۬لْفَلَقِ"
    },
    {
      "ayah_number": 2,
      "text": "مِن شَرِّ مَا خَلَقَ"
    },
    {
      "ayah_number": 3,
      "text": "وَمِن شَرِّ غَاسِقٍ اِذَا وَقَبَ"
    },
    {
      "ayah_number": 4,
      "text": "وَمِن شَرِّ اِ۬لنَّفَّٰثَٰتِ فِے اِ۬لْعُقَدِ"
    },
    {
      "ayah_number": 5,
      "text": "وَمِن شَرِّ حَاسِدٍ اِذَا حَسَدَۖ"
    }
  ]
}