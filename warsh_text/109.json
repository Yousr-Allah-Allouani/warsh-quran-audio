{
  "surah_number": 109,
  "name": "اُ۬لْكَٰفِرُونَ",
  "number_of_ayahs": 6,
  "ayahs": [
    {
      "ayah_number": 1,
      "text": "قُلْ يَٰٓأَيُّهَا اَ۬لْكَٰفِرُونَ"
    },
    {
      "ayah_number": 2,
      "text": "لَآ أَعْبُدُ مَا تَعْبُدُونَ"
    },
    {
      "ayah_number": 3,
      "text": "وَلَآ أَنتُمْ عَٰبِدُونَ مَآ أَعْبُدُۖ"
    },
    {
      "ayah_number": 4,
      "text": "وَلَآ أَنَا عَابِدٞ مَّا عَبَدتُّمْ"
    },
    {
      "ayah_number": 5,
      "text": "وَلَآ أَنتُمْ عَٰبِدُونَ مَآ أَعْبُدُۖ"
    },
    {
      "ayah_number": 6,
      "text": "لَكُمْ دِينُكُمْ وَلِيَ دِينِۖ"
    }
  ]
}