{
  "surah_number": 89,
  "name": "اُ۬لْفَجْرِ",
  "number_of_ayahs": 32,
  "ayahs": [
    {
      "ayah_number": 1,
      "text": "وَالْفَجْرِ"
    },
    {
      "ayah_number": 2,
      "text": "وَلَيَالٍ عَشْرٖ"
    },
    {
      "ayah_number": 3,
      "text": "وَالشَّفْعِ وَالْوَتْرِ"
    },
    {
      "ayah_number": 4,
      "text": "وَاليْلِ إِذَا يَسْرِۦ"
    },
    {
      "ayah_number": 5,
      "text": "هَلْ فِے ذَٰلِكَ قَسَمٞ لِّذِے حِجْرٍۖ"
    },
    {
      "ayah_number": 6,
      "text": "اَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ"
    },
    {
      "ayah_number": 7,
      "text": "اِرَمَ ذَاتِ اِ۬لْعِمَادِ"
    },
    {
      "ayah_number": 8,
      "text": "اِ۬لتِے لَمْ يُخْلَقْ مِثْلُهَا فِے اِ۬لْبِلَٰدِ"
    },
    {
      "ayah_number": 9,
      "text": "وَثَمُودَ اَ۬لذِينَ جَابُواْ اُ۬لصَّخْرَ بِالْوَادِۦ"
    },
    {
      "ayah_number": 10,
      "text": "وَفِرْعَوْنَ ذِے اِ۬لَاوْتَادِ"
    },
    {
      "ayah_number": 11,
      "text": "اِ۬لذِينَ طَغَوْاْ فِے اِ۬لْبِلَٰدِ"
    },
    {
      "ayah_number": 12,
      "text": "فَأَكْثَرُواْ فِيهَا اَ۬لْفَسَادَ"
    },
    {
      "ayah_number": 13,
      "text": "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍۖ"
    },
    {
      "ayah_number": 14,
      "text": "اِنَّ رَبَّكَ لَبِالْمِرْصَادِۖ"
    },
    {
      "ayah_number": 15,
      "text": "فَأَمَّا اَ۬لِانسَٰنُ إِذَا مَا اَ۪بْتَل۪يٰهُ رَبُّهُۥ فَأَكْرَمَهُۥ وَنَعَّمَهُۥ"
    },
    {
      "ayah_number": 16,
      "text": "فَيَقُولُ رَبِّيَ أَكْرَمَنِۦۖ"
    },
    {
      "ayah_number": 17,
      "text": "وَأَمَّآ إِذَا مَا اَ۪بْتَل۪يٰهُ فَقَدَرَ عَلَيْهِ رِزْقَهُۥ"
    },
    {
      "ayah_number": 18,
      "text": "فَيَقُولُ رَبِّيَ أَهَٰنَنِۦ"
    },
    {
      "ayah_number": 19,
      "text": "كَلَّاۖ بَل لَّا تُكْرِمُونَ اَ۬لْيَتِيمَ"
    },
    {
      "ayah_number": 20,
      "text": "وَلَا تَحُضُّونَ عَلَىٰ طَعَامِ اِ۬لْمِسْكِينِ"
    },
    {
      "ayah_number": 21,
      "text": "وَتَاكُلُونَ اَ۬لتُّرَاثَ أَكْلاٗ لَّمّاٗ"
    },
    {
      "ayah_number": 22,
      "text": "وَتُحِبُّونَ اَ۬لْمَالَ حُبّاٗ جَمّاٗ"
    },
    {
      "ayah_number": 23,
      "text": "كَلَّآۖ إِذَا دُكَّتِ اِ۬لَارْضُ دَكّاٗ دَكّاٗ"
    },
    {
      "ayah_number": 24,
      "text": "وَجَآءَ رَبُّكَ وَالْمَلَكُ صَفّاٗ صَفّاٗ"
    },
    {
      "ayah_number": 25,
      "text": "وَجِےٓءَ يَوْمَئِذِۢ بِجَهَنَّمَ"
    },
    {
      "ayah_number": 26,
      "text": "يَوْمَئِذٖ يَتَذَكَّرُ اُ۬لِانسَٰنُ وَأَنّ۪ىٰ لَهُ اُ۬لذِّكْر۪ىٰۖ"
    },
    {
      "ayah_number": 27,
      "text": "يَقُولُ يَٰلَيْتَنِے قَدَّمْتُ لِحَيَاتِےۖ"
    },
    {
      "ayah_number": 28,
      "text": "فَيَوْمَئِذٖ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَدٞ"
    },
    {
      "ayah_number": 29,
      "text": "وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَدٞۖ"
    },
    {
      "ayah_number": 30,
      "text": "يَٰٓأَيَّتُهَا اَ۬لنَّفْسُ اُ۬لْمُطْمَئِنَّةُ"
    },
    {
      "ayah_number": 31,
      "text": "اُ۪رْجِعِےٓ إِلَىٰ رَبِّكِ رَاضِيَةٗ مَّرْضِيَّةٗ"
    },
    {
      "ayah_number": 32,
      "text": "فَادْخُلِے فِے عِبَٰدِے وَادْخُلِے جَنَّتِےۖ"
    }
  ]
}