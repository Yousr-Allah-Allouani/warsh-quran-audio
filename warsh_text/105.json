{
  "surah_number": 105,
  "name": "اُ۬لْفِيلِ",
  "number_of_ayahs": 5,
  "ayahs": [
    {
      "ayah_number": 1,
      "text": "اَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِأَصْحَٰبِ اِ۬لْفِيلِۖ"
    },
    {
      "ayah_number": 2,
      "text": "أَلَمْ يَجْعَلْ كَيْدَهُمْ فِے تَضْلِيلٖ"
    },
    {
      "ayah_number": 3,
      "text": "وَأَرْسَلَ عَلَيْهِمْ طَيْراً اَبَابِيلَ"
    },
    {
      "ayah_number": 4,
      "text": "تَرْمِيهِم بِحِجَارَةٖ مِّن سِجِّيلٖ"
    },
    {
      "ayah_number": 5,
      "text": "فَجَعَلَهُمْ كَعَصْفٖ مَّاكُولٖۖ"
    }
  ]
}