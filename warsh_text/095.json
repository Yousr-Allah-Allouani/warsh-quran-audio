{
  "surah_number": 95,
  "name": "اُ۬لتِّينِ",
  "number_of_ayahs": 8,
  "ayahs": [
    {
      "ayah_number": 1,
      "text": "وَالتِّينِ وَالزَّيْتُونِ"
    },
    {
      "ayah_number": 2,
      "text": "وَطُورِ سِينِينَ"
    },
    {
      "ayah_number": 3,
      "text": "وَهَٰذَا اَ۬لْبَلَدِ اِ۬لَامِينِ"
    },
    {
      "ayah_number": 4,
      "text": "لَقَدْ خَلَقْنَا اَ۬لِانسَٰنَ فِےٓ أَحْسَنِ تَقْوِيمٖ"
    },
    {
      "ayah_number": 5,
      "text": "ثُمَّ رَدَدْنَٰهُ أَسْفَلَ سَٰفِلِينَ"
    },
    {
      "ayah_number": 6,
      "text": "إِلَّا اَ۬لذِينَ ءَامَنُواْ وَعَمِلُواْ اُ۬لصَّٰلِحَٰتِ فَلَهُمُۥٓ أَجْرٌ غَيْرُ مَمْنُونٖۖ"
    },
    {
      "ayah_number": 7,
      "text": "فَمَا يُكَذِّبُكَ بَعْدُ بِالدِّينِۖ"
    },
    {
      "ayah_number": 8,
      "text": "أَلَيْسَ اَ۬للَّهُ بِأَحْكَمِ اِ۬لْحَٰكِمِينَۖ"
    }
  ]
}