{
  "surah_number": 106,
  "name": "قُرَيْشٍ",
  "number_of_ayahs": 5,
  "ayahs": [
    {
      "ayah_number": 1,
      "text": "لِّإِيلَٰفِ قُرَيْشٍ"
    },
    {
      "ayah_number": 2,
      "text": "اِيلَٰفِهِمْ رِحْلَةَ اَ۬لشِّتَآءِ وَالصَّيْفِۖ"
    },
    {
      "ayah_number": 3,
      "text": "فَلْيَعْبُدُواْ رَبَّ هَٰذَا اَ۬لْبَيْتِ"
    },
    {
      "ayah_number": 4,
      "text": "اِ۬لذِےٓ أَطْعَمَهُم مِّن جُوعٖ"
    },
    {
      "ayah_number": 5,
      "text": "وَءَامَنَهُم مِّنْ خَوْفٍۖ"
    }
  ]
}