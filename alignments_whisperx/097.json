  "warsh_ayahs": [
    {
      "ayah_number": 1,
      "text": "اِنَّآ أَنزَلْنَٰهُ فِے لَيْلَةِ اِ۬لْقَدْرِۖ"
    },
    {
      "ayah_number": 2,
      "text": "وَمَآ أَدْر۪يٰكَ مَا لَيْلَةُ اُ۬لْقَدْرِۖ"
    },
    {
      "ayah_number": 3,
      "text": "لَيْلَةُ اُ۬لْقَدْرِ خَيْرٞ مِّنَ اَلْفِ شَهْرٖۖ"
    },
    {
      "ayah_number": 4,
      "text": "تَنَزَّلُ اُ۬لْمَلَٰٓئِكَةُ وَالرُّوحُ فِيهَا بِإِذْنِ رَبِّهِم مِّن كُلِّ أَمْرٖۖ"
    },
    {
      "ayah_number": 5,
      "text": "سَلَٰمٌ هِيَ حَتَّىٰ مَطْلَعِ اِ۬لْفَجْرِۖ"
    }
  ]
