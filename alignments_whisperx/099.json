  "warsh_ayahs": [
    {
      "ayah_number": 1,
      "text": "إِذَا زُلْزِلَتِ اِ۬لَارْضُ زِلْزَالَهَا"
    },
    {
      "ayah_number": 2,
      "text": "وَأَخْرَجَتِ اِ۬لَارْضُ أَثْقَالَهَا"
    },
    {
      "ayah_number": 3,
      "text": "وَقَالَ اَ۬لِانسَٰنُ مَا لَهَا"
    },
    {
      "ayah_number": 4,
      "text": "يَوْمَئِذٖ تُحَدِّثُ أَخْبَارَهَا"
    },
    {
      "ayah_number": 5,
      "text": "بِأَنَّ رَبَّكَ أَوْح۪ىٰ لَهَاۖ"
    },
    {
      "ayah_number": 6,
      "text": "يَوْمَئِذٖ يَصْدُرُ اُ۬لنَّاسُ أَشْتَاتاٗ"
    },
    {
      "ayah_number": 7,
      "text": "لِّيُرَوَاْ اَعْمَٰلَهُمْۖ"
    },
    {
      "ayah_number": 8,
      "text": "فَمَنْ يَّعْمَلْ مِثْقَالَ ذَرَّةٍ خَيْراٗ يَرَهُۥۖ"
    },
    {
      "ayah_number": 9,
      "text": "وَمَنْ يَّعْمَلْ مِثْقَالَ ذَرَّةٖ شَرّاٗ يَرَهُۥۖ"
    }
  ]
}