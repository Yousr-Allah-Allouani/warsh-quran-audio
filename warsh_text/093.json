{
  "surah_number": 93,
  "name": "اُ۬لضُّح۪يٰ",
  "number_of_ayahs": 11,
  "ayahs": [
    {
      "ayah_number": 1,
      "text": "وَالضُّح۪ىٰ"
    },
    {
      "ayah_number": 2,
      "text": "وَاليْلِ إِذَا سَج۪ىٰ"
    },
    {
      "ayah_number": 3,
      "text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَل۪ىٰۖ"
    },
    {
      "ayah_number": 4,
      "text": "وَلَلَاخِرَةُ خَيْرٞ لَّكَ مِنَ اَ۬لُاول۪ىٰۖ"
    },
    {
      "ayah_number": 5,
      "text": "وَلَسَوْفَ يُعْطِيكَ رَبُّكَ فَتَرْض۪ىٰٓۖ"
    },
    {
      "ayah_number": 6,
      "text": "أَلَمْ يَجِدْكَ يَتِيماٗ فَـَٔاو۪ىٰ"
    },
    {
      "ayah_number": 7,
      "text": "وَوَجَدَكَ ضَآلّاٗ فَهَد۪ىٰ"
    },
    {
      "ayah_number": 8,
      "text": "وَوَجَدَكَ عَآئِلاٗ فَأَغْن۪ىٰۖ"
    },
    {
      "ayah_number": 9,
      "text": "فَأَمَّا اَ۬لْيَتِيمَ فَلَا تَقْهَرْۖ"
    },
    {
      "ayah_number": 10,
      "text": "وَأَمَّا اَ۬لسَّآئِلَ فَلَا تَنْهَرْۖ"
    },
    {
      "ayah_number": 11,
      "text": "وَأَمَّا بِنِعْمَةِ رَبِّكَ فَحَدِّثَۖ"
    }
  ]
}