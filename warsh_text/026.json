{
  "surah_number": 26,
  "name": "اُ۬لشُّعَرَآءِ",
  "number_of_ayahs": 226,
  "ayahs": [
    {
      "ayah_number": 1,
      "text": "طَسِٓمِّٓۖ تِلْكَ ءَايَٰتُ اُ۬لْكِتَٰبِ اِ۬لْمُبِينِۖ"
    },
    {
      "ayah_number": 2,
      "text": "لَعَلَّكَ بَٰخِعٞ نَّفْسَكَ أَلَّا يَكُونُواْ مُومِنِينَۖ"
    },
    {
      "ayah_number": 3,
      "text": "إِن نَّشَأْ نُنَزِّلْ عَلَيْهِم مِّنَ اَ۬لسَّمَآءِ اَ۟يَةٗ فَظَلَّتَ اَعْنَٰقُهُمْ لَهَا خَٰضِعِينَۖ"
    },
    {
      "ayah_number": 4,
      "text": "وَمَا يَاتِيهِم مِّن ذِكْرٖ مِّنَ اَ۬لرَّحْمَٰنِ مُحْدَثٍ اِلَّا كَانُواْ عَنْهُ مُعْرِضِينَۖ"
    },
    {
      "ayah_number": 5,
      "text": "فَقَدْ كَذَّبُواْ فَسَيَاتِيهِمُۥٓ أَنۢبَٰٓؤُاْ مَا كَانُواْ بِهِۦ يَسْتَهْزِءُونَۖ"
    },
    {
      "ayah_number": 6,
      "text": "أَوَلَمْ يَرَوِاْ اِلَى اَ۬لَارْضِ كَمَ اَنۢبَتْنَا فِيهَا مِن كُلِّ زَوْجٖ كَرِيمٍۖ"
    },
    {
      "ayah_number": 7,
      "text": "اِنَّ فِے ذَٰلِكَ لَأٓيَةٗۖ وَمَا كَانَ أَكْثَرُهُم مُّومِنِينَۖ"
    },
    {
      "ayah_number": 8,
      "text": "وَإِنَّ رَبَّكَ لَهُوَ اَ۬لْعَزِيزُ اُ۬لرَّحِيمُۖ"
    },
    {
      "ayah_number": 9,
      "text": "وَإِذْ نَاد۪ىٰ رَبُّكَ مُوس۪ىٰٓ أَنِ اِ۪يتِ اِ۬لْقَوْمَ اَ۬لظَّٰلِمِينَ"
    },
    {
      "ayah_number": 10,
      "text": "قَوْمَ فِرْعَوْنَۖ أَلَا يَتَّقُونَۖ"
    },
    {
      "ayah_number": 11,
      "text": "قَالَ رَبِّ إِنِّيَ أَخَافُ أَنْ يُّكَذِّبُونِ"
    },
    {
      "ayah_number": 12,
      "text": "وَيَضِيقُ صَدْرِے وَلَا يَنطَلِقُ لِسَانِے فَأَرْسِلِ اِلَىٰ هَٰرُونَ"
    },
    {
      "ayah_number": 13,
      "text": "وَلَهُمْ عَلَيَّ ذَنۢبٞ فَأَخَافُ أَنْ يَّقْتُلُونِۖ"
    },
    {
      "ayah_number": 14,
      "text": "قَالَ كَلَّاۖ فَاذْهَبَا بِـَٔايَٰتِنَآ إِنَّا مَعَكُم مُّسْتَمِعُونَۖ"
    },
    {
      "ayah_number": 15,
      "text": "فَاتِيَا فِرْعَوْنَ فَقُولَآ إِنَّا رَسُولُ رَبِّ اِ۬لْعَٰلَمِينَ"
    },
    {
      "ayah_number": 16,
      "text": "أَنَ اَرْسِلْ مَعَنَا بَنِےٓ إِسْرَآءِيلَۖ"
    },
    {
      "ayah_number": 17,
      "text": "قَالَ أَلَمْ نُرَبِّكَ فِينَا وَلِيداٗ وَلَبِثْتَ فِينَا مِنْ عُمُرِكَ سِنِينَ"
    },
    {
      "ayah_number": 18,
      "text": "وَفَعَلْتَ فَعْلَتَكَ اَ۬لتِے فَعَلْتَ وَأَنتَ مِنَ اَ۬لْكٰ۪فِرِينَۖ"
    },
    {
      "ayah_number": 19,
      "text": "قَالَ فَعَلْتُهَآ إِذاٗ وَأَنَا مِنَ اَ۬لضَّآلِّينَۖ"
    },
    {
      "ayah_number": 20,
      "text": "فَفَرَرْتُ مِنكُمْ لَمَّا خِفْتُكُمْ فَوَهَبَ لِے رَبِّے حُكْماٗ وَجَعَلَنِے مِنَ اَ۬لْمُرْسَلِينَۖ"
    },
    {
      "ayah_number": 21,
      "text": "وَتِلْكَ نِعْمَةٞ تَمُنُّهَا عَلَيَّ أَنْ عَبَّدتَّ بَنِےٓ إِسْرَآءِيلَۖ"
    },
    {
      "ayah_number": 22,
      "text": "قَالَ فِرْعَوْنُ وَمَا رَبُّ اُ۬لْعَٰلَمِينَۖ"
    },
    {
      "ayah_number": 23,
      "text": "قَالَ رَبُّ اُ۬لسَّمَٰوَٰتِ وَالَارْضِ وَمَا بَيْنَهُمَآ إِن كُنتُم مُّوقِنِينَۖ"
    },
    {
      "ayah_number": 24,
      "text": "قَالَ لِمَنْ حَوْلَهُۥٓ أَلَا تَسْتَمِعُونَۖ"
    },
    {
      "ayah_number": 25,
      "text": "قَالَ رَبُّكُمْ وَرَبُّ ءَابَآئِكُمُ اُ۬لَاوَّلِينَۖ"
    },
    {
      "ayah_number": 26,
      "text": "قَالَ إِنَّ رَسُولَكُمُ اُ۬لذِےٓ أُرْسِلَ إِلَيْكُمْ لَمَجْنُونٞۖ"
    },
    {
      "ayah_number": 27,
      "text": "قَالَ رَبُّ اُ۬لْمَشْرِقِ وَالْمَغْرِبِ وَمَا بَيْنَهُمَآ إِن كُنتُمْ تَعْقِلُونَۖ"
    },
    {
      "ayah_number": 28,
      "text": "قَالَ لَئِنِ اِ۪تَّخَذتَّ إِلَٰهاً غَيْرِے لَأَجْعَلَنَّكَ مِنَ اَ۬لْمَسْجُونِينَۖ"
    },
    {
      "ayah_number": 29,
      "text": "قَالَ أَوَلَوْ جِئْتُكَ بِشَےْءٖ مُّبِينٖۖ"
    },
    {
      "ayah_number": 30,
      "text": "قَالَ فَاتِ بِهِۦٓ إِن كُنتَ مِنَ اَ۬لصَّٰدِقِينَۖ"
    },
    {
      "ayah_number": 31,
      "text": "فَأَلْق۪ىٰ عَصَاهُ فَإِذَا هِيَ ثُعْبَانٞ مُّبِينٞ"
    },
    {
      "ayah_number": 32,
      "text": "وَنَزَعَ يَدَهُۥ فَإِذَا هِيَ بَيْضَآءُ لِلنَّٰظِرِينَۖ"
    },
    {
      "ayah_number": 33,
      "text": "قَالَ لِلْمَلَإِ حَوْلَهُۥٓ إِنَّ هَٰذَا لَسَٰحِرٌ عَلِيمٞ"
    },
    {
      "ayah_number": 34,
      "text": "يُرِيدُ أَنْ يُّخْرِجَكُم مِّنَ اَرْضِكُم بِسِحْرِهِۦ فَمَاذَا تَامُرُونَۖ"
    },
    {
      "ayah_number": 35,
      "text": "قَالُوٓاْ أَرْجِهِۦ وَأَخَاهُ وَابْعَثْ فِے اِ۬لْمَدَآئِنِ حَٰشِرِينَ"
    },
    {
      "ayah_number": 36,
      "text": "يَاتُوكَ بِكُلِّ سَحّ۪ارٍ عَلِيمٖۖ"
    },
    {
      "ayah_number": 37,
      "text": "فَجُمِعَ اَ۬لسَّحَرَةُ لِمِيقَٰتِ يَوْمٖ مَّعْلُومٖۖ"
    },
    {
      "ayah_number": 38,
      "text": "وَقِيلَ لِلنَّاسِ هَلَ اَنتُم مُّجْتَمِعُونَ"
    },
    {
      "ayah_number": 39,
      "text": "لَعَلَّنَا نَتَّبِعُ اُ۬لسَّحَرَةَ إِن كَانُواْ هُمُ اُ۬لْغَٰلِبِينَۖ"
    },
    {
      "ayah_number": 40,
      "text": "فَلَمَّا جَآءَ اَ۬لسَّحَرَةُ قَالُواْ لِفِرْعَوْنَ أَئِنَّ لَنَا لَأَجْراً اِن كُنَّا نَحْنُ اُ۬لْغَٰلِبِينَۖ"
    },
    {
      "ayah_number": 41,
      "text": "قَالَ نَعَمْ وَإِنَّكُمُۥٓ إِذاٗ لَّمِنَ اَ۬لْمُقَرَّبِينَۖ"
    },
    {
      "ayah_number": 42,
      "text": "قَالَ لَهُم مُّوس۪ىٰٓ أَلْقُواْ مَآ أَنتُم مُّلْقُونَۖ"
    },
    {
      "ayah_number": 43,
      "text": "فَأَلْقَوْاْ حِبَالَهُمْ وَعِصِيَّهُمْ وَقَالُواْ بِعِزَّةِ فِرْعَوْنَ إِنَّا لَنَحْنُ اُ۬لْغَٰلِبُونَۖ"
    },
    {
      "ayah_number": 44,
      "text": "فَأَلْق۪ىٰ مُوس۪ىٰ عَصَاهُ فَإِذَا هِيَ تَلَقَّفُ مَا يَافِكُونَۖ"
    },
    {
      "ayah_number": 45,
      "text": "فَأُلْقِيَ اَ۬لسَّحَرَةُ سَٰجِدِينَۖ"
    },
    {
      "ayah_number": 46,
      "text": "قَالُوٓاْ ءَامَنَّا بِرَبِّ اِ۬لْعَٰلَمِينَۖ"
    },
    {
      "ayah_number": 47,
      "text": "رَبِّ مُوس۪ىٰ وَهَٰرُونَۖ"
    },
    {
      "ayah_number": 48,
      "text": "قَالَ ءَاٰ۬مَنتُمْ لَهُۥ قَبْلَ أَنَ اٰذَنَ لَكُمُۥٓ إِنَّهُۥ لَكَبِيرُكُمُ اُ۬لذِے عَلَّمَكُمُ اُ۬لسِّحْرَۖ فَلَسَوْفَ تَعْلَمُونَۖ"
    },
    {
      "ayah_number": 49,
      "text": "لَأُقَطِّعَنَّ أَيْدِيَكُمْ وَأَرْجُلَكُم مِّنْ خِلَٰفٖ وَلَأُصَلِّبَنَّكُمُۥٓ أَجْمَعِينَۖ"
    },
    {
      "ayah_number": 50,
      "text": "قَالُواْ لَا ضَيْرَۖ إِنَّآ إِلَىٰ رَبِّنَا مُنقَلِبُونَۖ"
    },
    {
      "ayah_number": 51,
      "text": "إِنَّا نَطْمَعُ أَنْ يَّغْفِرَ لَنَا رَبُّنَا خَطَٰيٰ۪نَآ أَن كُنَّآ أَوَّلَ اَ۬لْمُومِنِينَۖ"
    },
    {
      "ayah_number": 52,
      "text": "وَأَوْحَيْنَآ إِلَىٰ مُوس۪ىٰٓ أَنِ اِ۪سْرِ بِعِبَادِيَ إِنَّكُم مُّتَّبَعُونَۖ"
    },
    {
      "ayah_number": 53,
      "text": "فَأَرْسَلَ فِرْعَوْنُ فِے اِ۬لْمَدَآئِنِ حَٰشِرِينَۖ"
    },
    {
      "ayah_number": 54,
      "text": "إِنَّ هَٰٓؤُلَآءِ لَشِرْذِمَةٞ قَلِيلُونَ"
    },
    {
      "ayah_number": 55,
      "text": "وَإِنَّهُمْ لَنَا لَغَآئِظُونَ"
    },
    {
      "ayah_number": 56,
      "text": "وَإِنَّا لَجَمِيعٌ حَذِرُونَۖ"
    },
    {
      "ayah_number": 57,
      "text": "فَأَخْرَجْنَٰهُم مِّن جَنَّٰتٖ وَعُيُونٖ"
    },
    {
      "ayah_number": 58,
      "text": "وَكُنُوزٖ وَمَقَامٖ كَرِيمٖۖ"
    },
    {
      "ayah_number": 59,
      "text": "كَذَٰلِكَۖ وَأَوْرَثْنَٰهَا بَنِےٓ إِسْرَآءِيلَ"
    },
    {
      "ayah_number": 60,
      "text": "فَأَتْبَعُوهُم مُّشْرِقِينَۖ"
    },
    {
      "ayah_number": 61,
      "text": "فَلَمَّا تَرَٰٓءَا اَ۬لْجَمْعَٰنِ قَالَ أَصْحَٰبُ مُوس۪ىٰٓ إِنَّا لَمُدْرَكُونَۖ"
    },
    {
      "ayah_number": 62,
      "text": "قَالَ كَلَّآۖ إِنَّ مَعِے رَبِّے سَيَهْدِينِۖ"
    },
    {
      "ayah_number": 63,
      "text": "فَأَوْحَيْنَآ إِلَىٰ مُوس۪ىٰٓ أَنِ اِ۪ضْرِب بِّعَصَاكَ اَ۬لْبَحْرَ فَانفَلَقَ فَكَانَ كُلُّ فِرْقٖ كَالطَّوْدِ اِ۬لْعَظِيمِۖ"
    },
    {
      "ayah_number": 64,
      "text": "وَأَزْلَفْنَا ثَمَّ اَ۬لَاخَرِينَۖ"
    },
    {
      "ayah_number": 65,
      "text": "وَأَنجَيْنَا مُوس۪ىٰ وَمَن مَّعَهُۥٓ أَجْمَعِينَ"
    },
    {
      "ayah_number": 66,
      "text": "ثُمَّ أَغْرَقْنَا اَ۬لَاخَرِينَۖ"
    },
    {
      "ayah_number": 67,
      "text": "إِنَّ فِے ذَٰلِكَ لَأٓيَةٗۖ وَمَا كَانَ أَكْثَرُهُم مُّومِنِينَۖ"
    },
    {
      "ayah_number": 68,
      "text": "وَإِنَّ رَبَّكَ لَهُوَ اَ۬لْعَزِيزُ اُ۬لرَّحِيمُۖ"
    },
    {
      "ayah_number": 69,
      "text": "وَاتْلُ عَلَيْهِمْ نَبَأَ ا۪بْرَٰهِيمَ"
    },
    {
      "ayah_number": 70,
      "text": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِۦ مَا تَعْبُدُونَۖ"
    },
    {
      "ayah_number": 71,
      "text": "قَالُواْ نَعْبُدُ أَصْنَاماٗ فَنَظَلُّ لَهَا عَٰكِفِينَۖ"
    },
    {
      "ayah_number": 72,
      "text": "قَالَ هَلْ يَسْمَعُونَكُمُۥٓ إِذْ تَدْعُونَ"
    },
    {
      "ayah_number": 73,
      "text": "أَوْ يَنفَعُونَكُمُۥٓ أَوْ يَضُرُّونَۖ"
    },
    {
      "ayah_number": 74,
      "text": "قَالُواْ بَلْ وَجَدْنَآ ءَابَآءَنَا كَذَٰلِكَ يَفْعَلُونَۖ"
    },
    {
      "ayah_number": 75,
      "text": "قَالَ أَفَرَٰٓيْتُم مَّا كُنتُمْ تَعْبُدُونَ"
    },
    {
      "ayah_number": 76,
      "text": "أَنتُمْ وَءَابَآؤُكُمُ اُ۬لَاقْدَمُونَ"
    },
    {
      "ayah_number": 77,
      "text": "فَإِنَّهُمْ عَدُوّٞ لِّيَۖ إِلَّا رَبَّ اَ۬لْعَٰلَمِينَ"
    },
    {
      "ayah_number": 78,
      "text": "اَ۬لذِے خَلَقَنِے فَهُوَ يَهْدِينِۖ"
    },
    {
      "ayah_number": 79,
      "text": "وَالذِے هُوَ يُطْعِمُنِے وَيَسْقِينِ"
    },
    {
      "ayah_number": 80,
      "text": "وَإِذَا مَرِضْتُ فَهُوَ يَشْفِينِۖ"
    },
    {
      "ayah_number": 81,
      "text": "وَالذِے يُمِيتُنِے ثُمَّ يُحْيِينِۖ"
    },
    {
      "ayah_number": 82,
      "text": "وَالذِےٓ أَطْمَعُ أَنْ يَّغْفِرَ لِے خَطِيَٓٔتِے يَوْمَ اَ۬لدِّينِۖ"
    },
    {
      "ayah_number": 83,
      "text": "رَبِّ هَبْ لِے حُكْماٗ وَأَلْحِقْنِے بِالصَّٰلِحِينَۖ"
    },
    {
      "ayah_number": 84,
      "text": "وَاجْعَل لِّے لِسَانَ صِدْقٖ فِے اِ۬لَاخِرِينَۖ"
    },
    {
      "ayah_number": 85,
      "text": "وَاجْعَلْنِے مِنْ وَّرَثَةِ جَنَّةِ اِ۬لنَّعِيمِۖ"
    },
    {
      "ayah_number": 86,
      "text": "وَاغْفِرْ لِأَبِيَ إِنَّهُۥ كَانَ مِنَ اَ۬لضَّآلِّينَۖ"
    },
    {
      "ayah_number": 87,
      "text": "وَلَا تُخْزِنِے يَوْمَ يُبْعَثُونَ"
    },
    {
      "ayah_number": 88,
      "text": "يَوْمَ لَا يَنفَعُ مَالٞ وَلَا بَنُونَ"
    },
    {
      "ayah_number": 89,
      "text": "إِلَّا مَنَ اَتَى اَ۬للَّهَ بِقَلْبٖ سَلِيمٖۖ"
    },
    {
      "ayah_number": 90,
      "text": "وَأُزْلِفَتِ اِ۬لْجَنَّةُ لِلْمُتَّقِينَۖ"
    },
    {
      "ayah_number": 91,
      "text": "وَبُرِّزَتِ اِ۬لْجَحِيمُ لِلْغَاوِينَۖ"
    },
    {
      "ayah_number": 92,
      "text": "وَقِيلَ لَهُمُۥٓ أَيْنَ مَا كُنتُمْ تَعْبُدُونَ"
    },
    {
      "ayah_number": 93,
      "text": "مِن دُونِ اِ۬للَّهِۖ هَلْ يَنصُرُونَكُمُۥٓ أَوْ يَنتَصِرُونَۖ"
    },
    {
      "ayah_number": 94,
      "text": "فَكُبْكِبُواْ فِيهَا هُمْ وَالْغَاوُۥنَ"
    },
    {
      "ayah_number": 95,
      "text": "وَجُنُودُ إِبْلِيسَ أَجْمَعُونَۖ"
    },
    {
      "ayah_number": 96,
      "text": "قَالُواْ وَهُمْ فِيهَا يَخْتَصِمُونَ"
    },
    {
      "ayah_number": 97,
      "text": "تَاللَّهِ إِن كُنَّا لَفِے ضَلَٰلٖ مُّبِينٍ"
    },
    {
      "ayah_number": 98,
      "text": "اِذْ نُسَوِّيكُم بِرَبِّ اِ۬لْعَٰلَمِينَۖ"
    },
    {
      "ayah_number": 99,
      "text": "وَمَآ أَضَلَّنَآ إِلَّا اَ۬لْمُجْرِمُونَۖ"
    },
    {
      "ayah_number": 100,
      "text": "فَمَا لَنَا مِن شَٰفِعِينَ"
    },
    {
      "ayah_number": 101,
      "text": "وَلَا صَدِيقٍ حَمِيمٖۖ"
    },
    {
      "ayah_number": 102,
      "text": "فَلَوَ اَنَّ لَنَا كَرَّةٗ فَنَكُونَ مِنَ اَ۬لْمُومِنِينَۖ"
    },
    {
      "ayah_number": 103,
      "text": "إِنَّ فِے ذَٰلِكَ لَأٓيَةٗۖ وَمَا كَانَ أَكْثَرُهُم مُّومِنِينَۖ"
    },
    {
      "ayah_number": 104,
      "text": "وَإِنَّ رَبَّكَ لَهُوَ اَ۬لْعَزِيزُ اُ۬لرَّحِيمُۖ"
    },
    {
      "ayah_number": 105,
      "text": "كَذَّبَتْ قَوْمُ نُوحٍ اِ۬لْمُرْسَلِينَ"
    },
    {
      "ayah_number": 106,
      "text": "إِذْ قَالَ لَهُمُۥٓ أَخُوهُمْ نُوحٌ اَلَا تَتَّقُونَ"
    },
    {
      "ayah_number": 107,
      "text": "إِنِّے لَكُمْ رَسُولٌ اَمِينٞۖ"
    },
    {
      "ayah_number": 108,
      "text": "فَاتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِۖ"
    },
    {
      "ayah_number": 109,
      "text": "وَمَآ أَسْـَٔلُكُمْ عَلَيْهِ مِنَ اَجْرٍۖ اِنَ اَجْرِيَ إِلَّا عَلَىٰ رَبِّ اِ۬لْعَٰلَمِينَۖ"
    },
    {
      "ayah_number": 110,
      "text": "فَاتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِۖ"
    },
    {
      "ayah_number": 111,
      "text": "قَالُوٓاْ أَنُومِنُ لَكَ وَاتَّبَعَكَ اَ۬لَارْذَلُونَۖ"
    },
    {
      "ayah_number": 112,
      "text": "قَالَ وَمَا عِلْمِے بِمَا كَانُواْ يَعْمَلُونَۖ"
    },
    {
      "ayah_number": 113,
      "text": "إِنْ حِسَابُهُمُۥٓ إِلَّا عَلَىٰ رَبِّے لَوْ تَشْعُرُونَۖ"
    },
    {
      "ayah_number": 114,
      "text": "وَمَآ أَنَا بِطَارِدِ اِ۬لْمُومِنِينَ"
    },
    {
      "ayah_number": 115,
      "text": "إِنَ اَنَا إِلَّا نَذِيرٞ مُّبِينٞۖ"
    },
    {
      "ayah_number": 116,
      "text": "قَالُواْ لَئِن لَّمْ تَنتَهِ يَٰنُوحُ لَتَكُونَنَّ مِنَ اَ۬لْمَرْجُومِينَۖ"
    },
    {
      "ayah_number": 117,
      "text": "قَالَ رَبِّ إِنَّ قَوْمِے كَذَّبُونِ"
    },
    {
      "ayah_number": 118,
      "text": "فَافْتَحْ بَيْنِے وَبَيْنَهُمْ فَتْحاٗ وَنَجِّنِے وَمَن مَّعِيَ مِنَ اَ۬لْمُومِنِينَۖ"
    },
    {
      "ayah_number": 119,
      "text": "فَأَنجَيْنَٰهُ وَمَن مَّعَهُۥ فِے اِ۬لْفُلْكِ اِ۬لْمَشْحُونِۖ"
    },
    {
      "ayah_number": 120,
      "text": "ثُمَّ أَغْرَقْنَا بَعْدُ اُ۬لْبَاقِينَۖ"
    },
    {
      "ayah_number": 121,
      "text": "إِنَّ فِے ذَٰلِكَ لَأٓيَةٗۖ وَمَا كَانَ أَكْثَرُهُم مُّومِنِينَۖ"
    },
    {
      "ayah_number": 122,
      "text": "وَإِنَّ رَبَّكَ لَهُوَ اَ۬لْعَزِيزُ اُ۬لرَّحِيمُۖ"
    },
    {
      "ayah_number": 123,
      "text": "كَذَّبَتْ عَادٌ اِ۬لْمُرْسَلِينَ"
    },
    {
      "ayah_number": 124,
      "text": "إِذْ قَالَ لَهُمُۥٓ أَخُوهُمْ هُودٌ اَلَا تَتَّقُونَ"
    },
    {
      "ayah_number": 125,
      "text": "إِنِّے لَكُمْ رَسُولٌ اَمِينٞۖ"
    },
    {
      "ayah_number": 126,
      "text": "فَاتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِۖ"
    },
    {
      "ayah_number": 127,
      "text": "وَمَآ أَسْـَٔلُكُمْ عَلَيْهِ مِنَ اَجْرٍۖ اِنَ اَجْرِيَ إِلَّا عَلَىٰ رَبِّ اِ۬لْعَٰلَمِينَۖ"
    },
    {
      "ayah_number": 128,
      "text": "أَتَبْنُونَ بِكُلِّ رِيعٍ اٰيَةٗ تَعْبَثُونَ"
    },
    {
      "ayah_number": 129,
      "text": "وَتَتَّخِذُونَ مَصَانِعَ لَعَلَّكُمْ تَخْلُدُونَ"
    },
    {
      "ayah_number": 130,
      "text": "وَإِذَا بَطَشْتُم بَطَشْتُمْ جَبّ۪ارِينَۖ"
    },
    {
      "ayah_number": 131,
      "text": "فَاتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِۖ"
    },
    {
      "ayah_number": 132,
      "text": "وَاتَّقُواْ اُ۬لذِےٓ أَمَدَّكُم بِمَا تَعْلَمُونَ"
    },
    {
      "ayah_number": 133,
      "text": "أَمَدَّكُم بِأَنْعَٰمٖ وَبَنِينَ"
    },
    {
      "ayah_number": 134,
      "text": "وَجَنَّٰتٖ وَعُيُونٍۖ"
    },
    {
      "ayah_number": 135,
      "text": "اِنِّيَ أَخَافُ عَلَيْكُمْ عَذَابَ يَوْمٍ عَظِيمٖۖ"
    },
    {
      "ayah_number": 136,
      "text": "قَالُواْ سَوَآءٌ عَلَيْنَآ أَوَعَظْتَ أَمْ لَمْ تَكُن مِّنَ اَ۬لْوَٰعِظِينَ"
    },
    {
      "ayah_number": 137,
      "text": "إِنْ هَٰذَآ إِلَّا خُلُقُ اُ۬لَاوَّلِينَ"
    },
    {
      "ayah_number": 138,
      "text": "وَمَا نَحْنُ بِمُعَذَّبِينَۖ"
    },
    {
      "ayah_number": 139,
      "text": "فَكَذَّبُوهُ فَأَهْلَكْنَٰهُمُۥٓۖ إِنَّ فِے ذَٰلِكَ لَأٓيَةٗۖ وَمَا كَانَ أَكْثَرُهُم مُّومِنِينَۖ"
    },
    {
      "ayah_number": 140,
      "text": "وَإِنَّ رَبَّكَ لَهُوَ اَ۬لْعَزِيزُ اُ۬لرَّحِيمُۖ"
    },
    {
      "ayah_number": 141,
      "text": "كَذَّبَتْ ثَمُودُ اُ۬لْمُرْسَلِينَ"
    },
    {
      "ayah_number": 142,
      "text": "إِذْ قَالَ لَهُمُۥٓ أَخُوهُمْ صَٰلِحٌ اَلَا تَتَّقُونَ"
    },
    {
      "ayah_number": 143,
      "text": "إِنِّے لَكُمْ رَسُولٌ اَمِينٞۖ"
    },
    {
      "ayah_number": 144,
      "text": "فَاتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِۖ"
    },
    {
      "ayah_number": 145,
      "text": "وَمَآ أَسْـَٔلُكُمْ عَلَيْهِ مِنَ اَجْرٍۖ اِنَ اَجْرِيَ إِلَّا عَلَىٰ رَبِّ اِ۬لْعَٰلَمِينَۖ"
    },
    {
      "ayah_number": 146,
      "text": "أَتُتْرَكُونَ فِے مَا هَٰهُنَآ ءَامِنِينَ"
    },
    {
      "ayah_number": 147,
      "text": "فِے جَنَّٰتٖ وَعُيُونٖ"
    },
    {
      "ayah_number": 148,
      "text": "وَزُرُوعٖ وَنَخْلٖ طَلْعُهَا هَضِيمٞ"
    },
    {
      "ayah_number": 149,
      "text": "وَتَنْحِتُونَ مِنَ اَ۬لْجِبَالِ بُيُوتاٗ فَرِهِينَۖ"
    },
    {
      "ayah_number": 150,
      "text": "فَاتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِۖ"
    },
    {
      "ayah_number": 151,
      "text": "وَلَا تُطِيعُوٓاْ أَمْرَ اَ۬لْمُسْرِفِينَ"
    },
    {
      "ayah_number": 152,
      "text": "اَ۬لذِينَ يُفْسِدُونَ فِے اِ۬لَارْضِ وَلَا يُصْلِحُونَۖ"
    },
    {
      "ayah_number": 153,
      "text": "قَالُوٓاْ إِنَّمَآ أَنتَ مِنَ اَ۬لْمُسَحَّرِينَ"
    },
    {
      "ayah_number": 154,
      "text": "مَآ أَنتَ إِلَّا بَشَرٞ مِّثْلُنَا فَاتِ بِـَٔايَةٍ اِن كُنتَ مِنَ اَ۬لصَّٰدِقِينَۖ"
    },
    {
      "ayah_number": 155,
      "text": "قَالَ هَٰذِهِۦ نَاقَةٞ لَّهَا شِرْبٞۖ وَلَكُمْ شِرْبُ يَوْمٖ مَّعْلُومٖۖ"
    },
    {
      "ayah_number": 156,
      "text": "وَلَا تَمَسُّوهَا بِسُوٓءٖ فَيَاخُذَكُمْ عَذَابُ يَوْمٍ عَظِيمٖۖ"
    },
    {
      "ayah_number": 157,
      "text": "فَعَقَرُوهَا فَأَصْبَحُواْ نَٰدِمِينَ"
    },
    {
      "ayah_number": 158,
      "text": "فَأَخَذَهُمُ اُ۬لْعَذَابُۖ إِنَّ فِے ذَٰلِكَ لَأٓيَةٗۖ وَمَا كَانَ أَكْثَرُهُم مُّومِنِينَۖ"
    },
    {
      "ayah_number": 159,
      "text": "وَإِنَّ رَبَّكَ لَهُوَ اَ۬لْعَزِيزُ اُ۬لرَّحِيمُۖ"
    },
    {
      "ayah_number": 160,
      "text": "كَذَّبَتْ قَوْمُ لُوطٍ اِ۬لْمُرْسَلِينَ"
    },
    {
      "ayah_number": 161,
      "text": "إِذْ قَالَ لَهُمُۥٓ أَخُوهُمْ لُوطٌ اَلَا تَتَّقُونَ"
    },
    {
      "ayah_number": 162,
      "text": "إِنِّے لَكُمْ رَسُولٌ اَمِينٞۖ"
    },
    {
      "ayah_number": 163,
      "text": "فَاتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِۖ"
    },
    {
      "ayah_number": 164,
      "text": "وَمَآ أَسْـَٔلُكُمْ عَلَيْهِ مِنَ اَجْرٍۖ اِنَ اَجْرِيَ إِلَّا عَلَىٰ رَبِّ اِ۬لْعَٰلَمِينَۖ"
    },
    {
      "ayah_number": 165,
      "text": "أَتَاتُونَ اَ۬لذُّكْرَانَ مِنَ اَ۬لْعَٰلَمِينَ"
    },
    {
      "ayah_number": 166,
      "text": "وَتَذَرُونَ مَا خَلَقَ لَكُمْ رَبُّكُم مِّنَ اَزْوَٰجِكُمۖ بَلَ اَنتُمْ قَوْمٌ عَادُونَۖ"
    },
    {
      "ayah_number": 167,
      "text": "قَالُواْ لَئِن لَّمْ تَنتَهِ يَٰلُوطُ لَتَكُونَنَّ مِنَ اَ۬لْمُخْرَجِينَۖ"
    },
    {
      "ayah_number": 168,
      "text": "قَالَ إِنِّے لِعَمَلِكُم مِّنَ اَ۬لْقَالِينَۖ"
    },
    {
      "ayah_number": 169,
      "text": "رَبِّ نَجِّنِے وَأَهْلِے مِمَّا يَعْمَلُونَۖ"
    },
    {
      "ayah_number": 170,
      "text": "فَنَجَّيْنَٰهُ وَأَهْلَهُۥٓ أَجْمَعِينَ"
    },
    {
      "ayah_number": 171,
      "text": "إِلَّا عَجُوزاٗ فِے اِ۬لْغَٰبِرِينَ"
    },
    {
      "ayah_number": 172,
      "text": "ثُمَّ دَمَّرْنَا اَ۬لَاخَرِينَۖ"
    },
    {
      "ayah_number": 173,
      "text": "وَأَمْطَرْنَا عَلَيْهِم مَّطَراٗۖ فَسَآءَ مَطَرُ اُ۬لْمُنذَرِينَۖ"
    },
    {
      "ayah_number": 174,
      "text": "إِنَّ فِے ذَٰلِكَ لَأٓيَةٗۖ وَمَا كَانَ أَكْثَرُهُم مُّومِنِينَۖ"
    },
    {
      "ayah_number": 175,
      "text": "وَإِنَّ رَبَّكَ لَهُوَ اَ۬لْعَزِيزُ اُ۬لرَّحِيمُۖ"
    },
    {
      "ayah_number": 176,
      "text": "كَذَّبَ أَصْحَٰبُ لَيْكَةَ اَ۬لْمُرْسَلِينَ"
    },
    {
      "ayah_number": 177,
      "text": "إِذْ قَالَ لَهُمْ شُعَيْبٌ اَلَا تَتَّقُونَ"
    },
    {
      "ayah_number": 178,
      "text": "إِنِّے لَكُمْ رَسُولٌ اَمِينٞۖ"
    },
    {
      "ayah_number": 179,
      "text": "فَاتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِۖ"
    },
    {
      "ayah_number": 180,
      "text": "وَمَآ أَسْـَٔلُكُمْ عَلَيْهِ مِنَ اَجْرٍۖ اِنَ اَجْرِيَ إِلَّا عَلَىٰ رَبِّ اِ۬لْعَٰلَمِينَۖ"
    },
    {
      "ayah_number": 181,
      "text": "أَوْفُواْ اُ۬لْكَيْلَۖ وَلَا تَكُونُواْ مِنَ اَ۬لْمُخْسِرِينَۖ"
    },
    {
      "ayah_number": 182,
      "text": "وَزِنُواْ بِالْقُسْطَاسِ اِ۬لْمُسْتَقِيمِۖ"
    },
    {
      "ayah_number": 183,
      "text": "وَلَا تَبْخَسُواْ اُ۬لنَّاسَ أَشْيَآءَهُمْۖ وَلَا تَعْثَوْاْ فِے اِ۬لَارْضِ مُفْسِدِينَۖ"
    },
    {
      "ayah_number": 184,
      "text": "وَاتَّقُواْ اُ۬لذِے خَلَقَكُمْ وَالْجِبِلَّةَ اَ۬لَاوَّلِينَۖ"
    },
    {
      "ayah_number": 185,
      "text": "قَالُوٓاْ إِنَّمَآ أَنتَ مِنَ اَ۬لْمُسَحَّرِينَ"
    },
    {
      "ayah_number": 186,
      "text": "وَمَآ أَنتَ إِلَّا بَشَرٞ مِّثْلُنَا وَإِن نَّظُنُّكَ لَمِنَ اَ۬لْكَٰذِبِينَ"
    },
    {
      "ayah_number": 187,
      "text": "فَأَسْقِطْ عَلَيْنَا كِسْفاٗ مِّنَ اَ۬لسَّمَآءِ ان كُنتَ مِنَ اَ۬لصَّٰدِقِينَۖ"
    },
    {
      "ayah_number": 188,
      "text": "قَالَ رَبِّيَ أَعْلَمُ بِمَا تَعْمَلُونَۖ"
    },
    {
      "ayah_number": 189,
      "text": "فَكَذَّبُوهُ فَأَخَذَهُمْ عَذَابُ يَوْمِ اِ۬لظُّلَّةِۖ إِنَّهُۥ كَانَ عَذَابَ يَوْمٍ عَظِيمٍۖ"
    },
    {
      "ayah_number": 190,
      "text": "اِنَّ فِے ذَٰلِكَ لَأٓيَةٗۖ وَمَا كَانَ أَكْثَرُهُم مُّومِنِينَۖ"
    },
    {
      "ayah_number": 191,
      "text": "وَإِنَّ رَبَّكَ لَهُوَ اَ۬لْعَزِيزُ اُ۬لرَّحِيمُۖ"
    },
    {
      "ayah_number": 192,
      "text": "وَإِنَّهُۥ لَتَنزِيلُ رَبِّ اِ۬لْعَٰلَمِينَۖ"
    },
    {
      "ayah_number": 193,
      "text": "نَزَلَ بِهِ اِ۬لرُّوحُ اُ۬لَامِينُ"
    },
    {
      "ayah_number": 194,
      "text": "عَلَىٰ قَلْبِكَ لِتَكُونَ مِنَ اَ۬لْمُنذِرِينَ"
    },
    {
      "ayah_number": 195,
      "text": "بِلِسَانٍ عَرَبِيّٖ مُّبِينٖۖ"
    },
    {
      "ayah_number": 196,
      "text": "وَإِنَّهُۥ لَفِے زُبُرِ اِ۬لَاوَّلِينَۖ"
    },
    {
      "ayah_number": 197,
      "text": "أَوَلَمْ يَكُن لَّهُمُۥٓ ءَايَةً اَنْ يَّعْلَمَهُۥ عُلَمَٰٓؤُاْ بَنِےٓ إِسْرَآءِيلَۖ"
    },
    {
      "ayah_number": 198,
      "text": "وَلَوْ نَزَّلْنَٰهُ عَلَىٰ بَعْضِ اِ۬لَاعْجَمِينَ"
    },
    {
      "ayah_number": 199,
      "text": "فَقَرَأَهُۥ عَلَيْهِم مَّا كَانُواْ بِهِۦ مُومِنِينَۖ"
    },
    {
      "ayah_number": 200,
      "text": "كَذَٰلِكَ سَلَكْنَٰهُ فِے قُلُوبِ اِ۬لْمُجْرِمِينَ"
    },
    {
      "ayah_number": 201,
      "text": "لَا يُومِنُونَ بِهِۦ حَتَّىٰ يَرَوُاْ اُ۬لْعَذَابَ اَ۬لَالِيمَ"
    },
    {
      "ayah_number": 202,
      "text": "فَيَاتِيَهُم بَغْتَةٗ وَهُمْ لَا يَشْعُرُونَ"
    },
    {
      "ayah_number": 203,
      "text": "فَيَقُولُواْ هَلْ نَحْنُ مُنظَرُونَۖ"
    },
    {
      "ayah_number": 204,
      "text": "أَفَبِعَذَابِنَا يَسْتَعْجِلُونَۖ"
    },
    {
      "ayah_number": 205,
      "text": "أَفَرَٰٓيْتَ إِن مَّتَّعْنَٰهُمْ سِنِينَ"
    },
    {
      "ayah_number": 206,
      "text": "ثُمَّ جَآءَهُم مَّا كَانُواْ يُوعَدُونَ"
    },
    {
      "ayah_number": 207,
      "text": "مَآ أَغْن۪ىٰ عَنْهُم مَّا كَانُواْ يُمَتَّعُونَۖ"
    },
    {
      "ayah_number": 208,
      "text": "وَمَآ أَهْلَكْنَا مِن قَرْيَةٍ اِلَّا لَهَا مُنذِرُونَ"
    },
    {
      "ayah_number": 209,
      "text": "ذِكْر۪ىٰۖ وَمَا كُنَّا ظَٰلِمِينَۖ"
    },
    {
      "ayah_number": 210,
      "text": "وَمَا تَنَزَّلَتْ بِهِ اِ۬لشَّيَٰطِينُ وَمَا يَنۢبَغِے لَهُمْ وَمَا يَسْتَطِيعُونَۖ"
    },
    {
      "ayah_number": 211,
      "text": "إِنَّهُمْ عَنِ اِ۬لسَّمْعِ لَمَعْزُولُونَۖ"
    },
    {
      "ayah_number": 212,
      "text": "فَلَا تَدْعُ مَعَ اَ۬للَّهِ إِلَٰهاً اٰخَرَ فَتَكُونَ مِنَ اَ۬لْمُعَذَّبِينَۖ"
    },
    {
      "ayah_number": 213,
      "text": "وَأَنذِرْ عَشِيرَتَكَ اَ۬لَاقْرَبِينَ"
    },
    {
      "ayah_number": 214,
      "text": "وَاخْفِضْ جَنَاحَكَ لِمَنِ اِ۪تَّبَعَكَ مِنَ اَ۬لْمُومِنِينَۖ"
    },
    {
      "ayah_number": 215,
      "text": "فَإِنْ عَصَوْكَ فَقُلِ اِنِّے بَرِےٓءٞ مِّمَّا تَعْمَلُونَۖ"
    },
    {
      "ayah_number": 216,
      "text": "فَتَوَكَّلْ عَلَى اَ۬لْعَزِيزِ اِ۬لرَّحِيمِ"
    },
    {
      "ayah_number": 217,
      "text": "اِ۬لذِے يَر۪يٰكَ حِينَ تَقُومُ"
    },
    {
      "ayah_number": 218,
      "text": "وَتَقَلُّبَكَ فِے اِ۬لسَّٰجِدِينَۖ"
    },
    {
      "ayah_number": 219,
      "text": "إِنَّهُۥ هُوَ اَ۬لسَّمِيعُ اُ۬لْعَلِيمُۖ"
    },
    {
      "ayah_number": 220,
      "text": "هَلُ ا۟نَبِّئُكُمْ عَلَىٰ مَن تَنَزَّلُ اُ۬لشَّيَٰطِينُ"
    },
    {
      "ayah_number": 221,
      "text": "تَنَزَّلُ عَلَىٰ كُلِّ أَفَّاكٍ اَثِيمٖ"
    },
    {
      "ayah_number": 222,
      "text": "يُلْقُونَ اَ۬لسَّمْعَ وَأَكْثَرُهُمْ كَٰذِبُونَۖ"
    },
    {
      "ayah_number": 223,
      "text": "وَالشُّعَرَآءُ يَتْبَعُهُمُ اُ۬لْغَاوُۥنَۖ"
    },
    {
      "ayah_number": 224,
      "text": "أَلَمْ تَرَ أَنَّهُمْ فِے كُلِّ وَادٖ يَهِيمُونَ"
    },
    {
      "ayah_number": 225,
      "text": "وَأَنَّهُمْ يَقُولُونَ مَا لَا يَفْعَلُونَۖ"
    },
    {
      "ayah_number": 226,
      "text": "إِلَّا اَ۬لذِينَ ءَامَنُواْ وَعَمِلُواْ اُ۬لصَّٰلِحَٰتِ وَذَكَرُواْ اُ۬للَّهَ كَثِيراٗ وَانتَصَرُواْ مِنۢ بَعْدِ مَا ظُلِمُواْۖ وَسَيَعْلَمُ اُ۬لذِينَ ظَلَمُوٓاْ أَيَّ مُنقَلَبٖ يَنقَلِبُونَۖ"
    }
  ]
}