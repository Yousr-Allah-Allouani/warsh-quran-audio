{
  "surah_number": 96,
  "name": "اُ۬لْعَلَقِ",
  "number_of_ayahs": 20,
  "ayahs": [
    {
      "ayah_number": 1,
      "text": "اَ۪قْرَأْ بِاسْمِ رَبِّكَ اَ۬لذِے خَلَقَۖ"
    },
    {
      "ayah_number": 2,
      "text": "خَلَقَ اَ۬لِانسَٰنَ مِنْ عَلَقٍۖ"
    },
    {
      "ayah_number": 3,
      "text": "اِ۪قْرَأْ وَرَبُّكَ اَ۬لَاكْرَمُ"
    },
    {
      "ayah_number": 4,
      "text": "اُ۬لذِے عَلَّمَ بِالْقَلَمِ"
    },
    {
      "ayah_number": 5,
      "text": "عَلَّمَ اَ۬لِانسَٰنَ مَا لَمْ يَعْلَمْۖ"
    },
    {
      "ayah_number": 6,
      "text": "كَلَّآ إِنَّ اَ۬لِانسَٰنَ لَيَطْغ۪ىٰٓ"
    },
    {
      "ayah_number": 7,
      "text": "أَن رّ۪ء۪اهُ اُ۪سْتَغْن۪ىٰٓۖ"
    },
    {
      "ayah_number": 8,
      "text": "إِنَّ إِلَىٰ رَبِّكَ اَ۬لرُّجْع۪ىٰٓۖ"
    },
    {
      "ayah_number": 9,
      "text": "أَرَٰٓيْتَ اَ۬لذِے يَنْه۪ىٰ"
    },
    {
      "ayah_number": 10,
      "text": "عَبْداً اِذَا صَلّ۪ىٰٓۖ"
    },
    {
      "ayah_number": 11,
      "text": "أَرَٰٓيْتَ إِن كَانَ عَلَى اَ۬لْهُد۪ىٰٓ"
    },
    {
      "ayah_number": 12,
      "text": "أَوَ اَمَرَ بِالتَّقْو۪ىٰٓۖ"
    },
    {
      "ayah_number": 13,
      "text": "أَرَٰٓيْتَ إِن كَذَّبَ وَتَوَلّ۪ىٰٓ"
    },
    {
      "ayah_number": 14,
      "text": "أَلَمْ يَعْلَم بِأَنَّ اَ۬للَّهَ يَر۪ىٰۖ"
    },
    {
      "ayah_number": 15,
      "text": "كَلَّا لَئِن لَّمْ يَنتَهِ"
    },
    {
      "ayah_number": 16,
      "text": "لَنَسْفَعاَۢ بِالنَّاصِيَةِ"
    },
    {
      "ayah_number": 17,
      "text": "نَاصِيَةٖ كَٰذِبَةٍ خَاطِئَةٖۖ"
    },
    {
      "ayah_number": 18,
      "text": "فَلْيَدْعُ نَادِيَهُۥ"
    },
    {
      "ayah_number": 19,
      "text": "سَنَدْعُ اُ۬لزَّبَانِيَةَۖ"
    },
    {
      "ayah_number": 20,
      "text": "كَلَّا لَا تُطِعْهُ وَاسْجُدْ وَاقْتَرِبِۖ۩"
    }
  ]
}