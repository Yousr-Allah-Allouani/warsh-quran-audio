{
  "surah_number": 112,
  "name": "اُ۬لِاخْلَاصِ",
  "number_of_ayahs": 4,
  "ayahs": [
    {
      "ayah_number": 1,
      "text": "قُلْ هُوَ اَ۬للَّهُ أَحَدٌۖ"
    },
    {
      "ayah_number": 2,
      "text": "اِ۬للَّهُ اُ۬لصَّمَدُۖ"
    },
    {
      "ayah_number": 3,
      "text": "لَمْ يَلِدْ وَلَمْ يُولَدْۖ"
    },
    {
      "ayah_number": 4,
      "text": "وَلَمْ يَكُن لَّهُۥ كُفُؤاً اَحَدٞۖ"
    }
  ]
