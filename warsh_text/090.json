{
  "surah_number": 90,
  "name": "اُ۬لْبَلَدِ",
  "number_of_ayahs": 20,
  "ayahs": [
    {
      "ayah_number": 1,
      "text": "لَآ أُقْسِمُ بِهَٰذَا اَ۬لْبَلَدِ"
    },
    {
      "ayah_number": 2,
      "text": "وَأَنتَ حِلُّۢ بِهَٰذَا اَ۬لْبَلَدِ"
    },
    {
      "ayah_number": 3,
      "text": "وَوَالِدٖ وَمَا وَلَدَ"
    },
    {
      "ayah_number": 4,
      "text": "لَقَدْ خَلَقْنَا اَ۬لِانسَٰنَ فِے كَبَدٍۖ"
    },
    {
      "ayah_number": 5,
      "text": "اَيَحْسِبُ أَن لَّنْ يَّقْدِرَ عَلَيْهِ أَحَدٞ"
    },
    {
      "ayah_number": 6,
      "text": "يَقُولُ أَهْلَكْتُ مَالاٗ لُّبَداًۖ"
    },
    {
      "ayah_number": 7,
      "text": "اَيَحْسِبُ أَن لَّمْ يَرَهُۥٓ أَحَدٌۖ"
    },
    {
      "ayah_number": 8,
      "text": "اَلَمْ نَجْعَل لَّهُۥ عَيْنَيْنِ"
    },
    {
      "ayah_number": 9,
      "text": "وَلِسَاناٗ وَشَفَتَيْنِ"
    },
    {
      "ayah_number": 10,
      "text": "وَهَدَيْنَٰهُ اُ۬لنَّجْدَيْنِۖ"
    },
    {
      "ayah_number": 11,
      "text": "فَلَا اَ۪قْتَحَمَ اَ۬لْعَقَبَةَۖ"
    },
    {
      "ayah_number": 12,
      "text": "وَمَآ أَدْر۪يٰكَ مَا اَ۬لْعَقَبَةُۖ"
    },
    {
      "ayah_number": 13,
      "text": "فَكُّ رَقَبَةٍ"
    },
    {
      "ayah_number": 14,
      "text": "اَوِ اِطْعَامٞ فِے يَوْمٖ ذِے مَسْغَبَةٖ"
    },
    {
      "ayah_number": 15,
      "text": "يَتِيماٗ ذَا مَقْرَبَةٍ"
    },
    {
      "ayah_number": 16,
      "text": "اَوْ مِسْكِيناٗ ذَا مَتْرَبَةٖۖ"
    },
    {
      "ayah_number": 17,
      "text": "ثُمَّ كَانَ مِنَ اَ۬لذِينَ ءَامَنُواْ وَتَوَاصَوْاْ بِالصَّبْرِ وَتَوَاصَوْاْ بِالْمَرْحَمَةِۖ"
    },
    {
      "ayah_number": 18,
      "text": "أُوْلَٰٓئِكَ أَصْحَٰبُ اُ۬لْمَيْمَنَةِۖ"
    },
    {
      "ayah_number": 19,
      "text": "وَالذِينَ كَفَرُواْ بِـَٔايَٰتِنَا هُمُۥٓ أَصْحَٰبُ اُ۬لْمَشْـَٔمَةِۖ"
    },
    {
      "ayah_number": 20,
      "text": "عَلَيْهِمْ نَارٞ مُّوصَدَةٞۖ"
    }
  ]
}