{
  "surah_number": 111,
  "name": "اُ۬لْمَسَدِ",
  "number_of_ayahs": 5,
  "ayahs": [
    {
      "ayah_number": 1,
      "text": "تَبَّتْ يَدَآ أَبِے لَهَبٖ وَتَبَّۖ"
    },
    {
      "ayah_number": 2,
      "text": "مَآ أَغْن۪ىٰ عَنْهُ مَالُهُۥ وَمَا كَسَبَۖ"
    },
    {
      "ayah_number": 3,
      "text": "سَيَصْلَىٰ نَاراٗ ذَاتَ لَهَبٖ"
    },
    {
      "ayah_number": 4,
      "text": "وَامْرَأَتُهُۥۖ حَمَّالَةُ اُ۬لْحَطَبِ"
    },
    {
      "ayah_number": 5,
      "text": "فِے جِيدِهَا حَبْلٞ مِّن مَّسَدٖۖ"
    }
  ]
}