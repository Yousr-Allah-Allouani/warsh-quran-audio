  "warsh_ayahs": [
    {
      "ayah_number": 1,
      "text": "وَيْلٞ لِّكُلِّ هُمَزَةٖ لُّمَزَةٍ"
    },
    {
      "ayah_number": 2,
      "text": "اِ۬لذِے جَمَعَ مَالاٗ وَعَدَّدَهُۥ"
    },
    {
      "ayah_number": 3,
      "text": "يَحْسِبُ أَنَّ مَالَهُۥٓ أَخْلَدَهُۥ"
    },
    {
      "ayah_number": 4,
      "text": "كَلَّاۖ لَيُنۢبَذَنَّ فِے اِ۬لْحُطَمَةِۖ"
    },
    {
      "ayah_number": 5,
      "text": "وَمَآ أَدْر۪يٰكَ مَا اَ۬لْحُطَمَةُۖ"
    },
    {
      "ayah_number": 6,
      "text": "نَارُ اُ۬للَّهِ اِ۬لْمُوقَدَةُ"
    },
    {
      "ayah_number": 7,
      "text": "اُ۬لتِے تَطَّلِعُ عَلَى اَ۬لَافْـِٕدَةِۖ"
    },
    {
      "ayah_number": 8,
      "text": "إِنَّهَا عَلَيْهِم مُّوصَدَةٞ"
    },
    {
      "ayah_number": 9,
      "text": "فِے عَمَدٖ مُّمَدَّدَةٍۖ"
    }
  ]
}